{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      name: '',\n      description: '',\n      price: '',\n      image: null\n    };\n  },\n  methods: {\n    handleImageUpload(event) {\n      this.image = event.target.files[0];\n    },\n    async addProduct() {\n      const formData = new FormData();\n      formData.append('name', this.name);\n      formData.append('description', this.description);\n      formData.append('price', this.price);\n      formData.append('image', this.image);\n      try {\n        const response = await this.$http.post('/admin/add-product', formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        alert(response.data.message);\n\n        // After successful product addition, redirect to the product page\n        if (response.data.success) {\n          this.$router.push('/product'); // Redirects to the /product page\n        }\n      } catch (error) {\n        console.error('Error adding product:', error);\n        alert('Failed to add product. Please try again.');\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","name","description","price","image","methods","handleImageUpload","event","target","files","addProduct","formData","FormData","append","response","$http","post","headers","alert","message","success","$router","push","error","console"],"sources":["C:\\laragon\\www\\admin_ecommerce\\vue\\src\\views\\admin.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>Add New Product</h1>\r\n    <form @submit.prevent=\"addProduct\">\r\n      <div>\r\n        <label for=\"name\">Product Name</label>\r\n        <input type=\"text\" v-model=\"name\" id=\"name\" required />\r\n      </div>\r\n      <div>\r\n        <label for=\"description\">Description</label>\r\n        <textarea v-model=\"description\" id=\"description\" required></textarea>\r\n      </div>\r\n      <div>\r\n        <label for=\"price\">Price</label>\r\n        <input type=\"number\" v-model=\"price\" id=\"price\" required />\r\n      </div>\r\n      <div>\r\n        <label for=\"image\">Product Image</label>\r\n        <input type=\"file\" @change=\"handleImageUpload\" required />\r\n      </div>\r\n      <button type=\"submit\">Add Product</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      name: '',\r\n      description: '',\r\n      price: '',\r\n      image: null\r\n    };\r\n  },\r\n  methods: {\r\n    handleImageUpload(event) {\r\n      this.image = event.target.files[0];\r\n    },\r\n    async addProduct() {\r\n      const formData = new FormData();\r\n      formData.append('name', this.name);\r\n      formData.append('description', this.description);\r\n      formData.append('price', this.price);\r\n      formData.append('image', this.image);\r\n\r\n      try {\r\n        const response = await this.$http.post('/admin/add-product', formData, {\r\n          headers: { 'Content-Type': 'multipart/form-data' }\r\n        });\r\n\r\n        alert(response.data.message);\r\n        \r\n        // After successful product addition, redirect to the product page\r\n        if (response.data.success) {\r\n          this.$router.push('/product');  // Redirects to the /product page\r\n        }\r\n      } catch (error) {\r\n        console.error('Error adding product:', error);\r\n        alert('Failed to add product. Please try again.');\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":";AA0BA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,iBAAiBA,CAACC,KAAK,EAAE;MACvB,IAAI,CAACH,KAAI,GAAIG,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IACpC,CAAC;IACD,MAAMC,UAAUA,CAAA,EAAG;MACjB,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/BD,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACZ,IAAI,CAAC;MAClCU,QAAQ,CAACE,MAAM,CAAC,aAAa,EAAE