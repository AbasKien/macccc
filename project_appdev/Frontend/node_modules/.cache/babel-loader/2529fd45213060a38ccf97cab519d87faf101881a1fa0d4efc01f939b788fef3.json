utton Styles */\r\n  .hero-btn {\r\n    background-color: #ffb547; /* Yellow */\r\n    border: none;\r\n    padding: 15px 30px;\r\n    font-size: 1.2rem;\r\n    color: white;\r\n    text-transform: uppercase;\r\n    border-radius: 5px;\r\n    text-decoration: none;\r\n  }\r\n  \r\n  .hero-btn:hover {\r\n    background-color: #ff9f33;\r\n    text-decoration: none;\r\n  }\r\n  </style>\r\n  "],"mappings":"AA4DE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO,CAAC,CAAC;EACX;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}