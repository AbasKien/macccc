{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.reduce.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      form: {\n        name: \"\",\n        address: \"\",\n        payment: \"\"\n      },\n      shipping: \"58\" // Default shipping cost\n    };\n  },\n  computed: {\n    totalItems() {\n      const cart = JSON.parse(localStorage.getItem('cart')) || [];\n      return cart.reduce((sum, item) => sum + item.quantity, 0);\n    },\n    totalPrice() {\n      const cart = JSON.parse(localStorage.getItem('cart')) || [];\n      return cart.reduce((total, item) => total + item.price * item.quantity, 0).toFixed(2);\n    }\n  },\n  methods: {\n    async checkout() {\n      try {\n        const response = await axios.post('http://localhost:5555/orders', {\n          name: this.form.name,\n          address: this.form.address,\n          payment: this.form.payment,\n          total_price: (parseFloat(this.totalPrice) + parseFloat(this.shipping)).toFixed(2) // Adding shipping\n        });\n        alert(response.data.message); // Display success message\n        this.$router.push(\"/thank-you\"); // Redirect to thank you page\n      } catch (error) {\n        console.error(\"Failed to checkout:\", error);\n        alert(\"There was an error placing your order. Please try again.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","form","name","address","payment","shipping","computed","totalItems","cart","JSON","parse","localStorage","getItem","reduce","sum","item","quantity","totalPrice","total","price","toFixed","methods","checkout","response","post","total_price","parseFloat","alert","message","$router","push","error","console"],"sources":["C:\\laragon\\www\\project_appdev\\Frontend\\src\\views\\cart.vue"],"sourcesContent":["<template>\r\n  <div class=\"checkout-page\">\r\n    <div class=\"checkout-container\">\r\n      <!-- Personal Information Section -->\r\n      <div class=\"personal-info\">\r\n        <h2 class=\"section-title\">Personal Information</h2>\r\n        <form @submit.prevent=\"checkout\" class=\"checkout-form\">\r\n          <div class=\"form-group\">\r\n            <label for=\"name\">Full Name</label>\r\n            <input type=\"text\" v-model=\"form.name\" required placeholder=\"Enter your full name\" />\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"address\">Shipping Address</label>\r\n            <textarea v-model=\"form.address\" required placeholder=\"Enter your shipping address\"></textarea>\r\n          </div>\r\n        </form>\r\n      </div>\r\n\r\n      <!-- Order Summary Section -->\r\n      <div class=\"order-summary\">\r\n        <h2 class=\"section-title\">Order Summary</h2>\r\n        <div class=\"summary-item\">\r\n          <span>ITEMS</span>\r\n          <span>{{ totalItems }}</span>\r\n        </div>\r\n        <div class=\"summary-item\">\r\n          <span>Total Price</span>\r\n          <span>₱{{ totalPrice }}</span>\r\n        </div>\r\n        <div class=\"summary-item\">\r\n          <label for=\"shipping\">Shipping</label>\r\n          <select id=\"shipping\" v-model=\"shipping\" class=\"shipping-select\">\r\n            <option value=\"58\">Standard Delivery - ₱58.00</option>\r\n          </select>\r\n        </div>\r\n        <div class=\"summary-total\">\r\n          <span>Total Amount</span>\r\n          <span>₱{{ (parseFloat(totalPrice) + parseFloat(shipping)).toFixed(2) }}</span>\r\n        </div>\r\n        <button @click=\"checkout\" class=\"checkout-button\">Place Order</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      form: {\r\n        name: \"\",\r\n        address: \"\",\r\n        payment: \"\",\r\n      },\r\n      shipping: \"58\", // Default shipping cost\r\n    };\r\n  },\r\n  computed: {\r\n    totalItems() {\r\n      const cart = JSON.parse(localStorage.getItem('cart')) || [];\r\n      return cart.reduce((sum, item) => sum + item.quantity, 0);\r\n    },\r\n    totalPrice() {\r\n      const cart = JSON.parse(localStorage.getItem('cart')) || [];\r\n      return cart.reduce((total, item) => total + item.price * item.quantity, 0).toFixed(2);\r\n    },\r\n  },\r\n  methods: {\r\n    async checkout() {\r\n      try {\r\n        const response = await axios.post('http://localhost:5555/orders', {\r\n          name: this.form.name,\r\n          address: this.form.address,\r\n          payment: this.form.payment,\r\n          total_price: (parseFloat(this.totalPrice) + parseFloat(this.shipping)).toFixed(2),  // Adding shipping\r\n        });\r\n\r\n        alert(response.data.message);  // Display success message\r\n        this.$router.push(\"/thank-you\"); // Redirect to thank you page\r\n      } catch (error) {\r\n        console.error(\"Failed to checkout:\", error);\r\n        alert(\"There was an error placing your order. Please try again.\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.checkout-page {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: flex-start;\r\n  padding: 30px;\r\n  background-color: #f4f4f4;\r\n}\r\n\r\n.checkout-container {\r\n  display: flex;\r\n  width: 90%;\r\n  max-width: 1200px;\r\n  gap: 30px;\r\n}\r\n\r\n.personal-info {\r\n  flex: 2;\r\n  background-color: #fff;\r\n  padding: 40px;\r\n  border-radius: 10px;\r\n  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.section-title {\r\n  font-size: 1.8rem;\r\n  font-weight: bold;\r\n  margin-bottom: 25px;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 20px;\r\n}\r\n\r\nlabel {\r\n  font-size: 1rem;\r\n  color: #666;\r\n  margin-bottom: 8px;\r\n}\r\n\r\ninput,\r\ntextarea,\r\nselect {\r\n  width: 100%;\r\n  padding: 12px;\r\n  font-size: 1rem;\r\n  border: 1px solid #ccc;\r\n  border-radius: 8px;\r\n  background-color: #f9f9f9;\r\n  margin-top: 8px;\r\n}\r\n\r\ninput:focus,\r\ntextarea:focus,\r\nselect:focus {\r\n  border-color: #4caf50;\r\n  outline: none;\r\n}\r\n\r\ntextarea {\r\n  min-height: 120px;\r\n  resize: vertical;\r\n}\r\n\r\n.order-summary {\r\n  flex: 1;\r\n  background-color: #f7f7f7;\r\n  padding: 30px;\r\n  border-radius: 10px;\r\n  text-align: left;\r\n  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.summary-item {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  font-size: 1rem;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.shipping-select {\r\n  width: 100%;\r\n  padding: 8px;\r\n  margin-top: 10px;\r\n  border: 1px solid #ddd;\r\n  background-color: #fff;\r\n  border-radius: 5px;\r\n}\r\n\r\n.summary-total {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  font-size: 1.2rem;\r\n  font-weight: bold;\r\n  margin-top: 30px;\r\n}\r\n\r\n.checkout-button {\r\n  width: 100%;\r\n  padding: 15px;\r\n  margin-top: 20px;\r\n  background-color: #000;\r\n  color: #fff;\r\n  font-size: 1.1rem;\r\n  border: none;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n  text-transform: uppercase;\r\n}\r\n\r\n.checkout-button:hover {\r\n  background-color: #333;\r\n}\r\n</style>\r\n"],"mappings":";;;AA8CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE;MACX,CAAC;MACDC,QAAQ,EAAE,IAAI,CAAE;IAClB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,MAAMC,IAAG,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,KAAK,EAAE;MAC3D,OAAOJ,IAAI,CAACK,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAKD,GAAE,GAAIC,IAAI,CAACC,QAAQ,EAAE,CAAC,CAAC;IAC3D,CAAC;IACDC,UAAUA,CAAA,EAAG;MACX,MAAMT,IAAG,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,KAAK,EAAE;MAC3D,OAAOJ,IAAI,CAACK,MAAM,CAAC,CAACK,KAAK,EAAEH,IAAI,KAAKG,KAAI,GAAIH,IAAI,CAACI,KAAI,GAAIJ,IAAI,CAACC,QAAQ,EAAE,CAAC,CAAC,CAACI,OAAO,CAAC,CAAC,CAAC;IACvF;EACF,CAAC;EACDC,O