{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"LoginComponent\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      rememberMe: false,\n      error: null\n    };\n  },\n  methods: {\n    async handleLogin() {\n      this.error = null;\n\n      // Check if email and password are filled before sending request\n      if (!this.email || !this.password) {\n        this.error = \"Please enter both email and password.\";\n        return;\n      }\n      try {\n        // Make the login request\n        const response = await axios.post('http://localhost:5555/login', {\n          email: this.email,\n          password: this.password,\n          rememberMe: this.rememberMe\n        });\n\n        // Check if user is verified\n        if (response.data.verified) {\n          // Redirect to the home page if verified\n          this.$router.push(\"/home\");\n        } else {\n          // If not verified, show error message\n          this.error = 'Please verify your Gmail before logging in.';\n        }\n      } catch (err) {\n        // Handle different errors\n        if (err.response && err.response.status === 401) {\n          this.error = err.response.data.message || \"Invalid email or password.\";\n        } else {\n          this.error = err.response?.data?.message || \"Login failed. Please try again.\";\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","rememberMe","error","methods","handleLogin","response","post","verified","$router","push","err","status","message"],"sources":["C:\\laragon\\www\\project_appdev\\Frontend\\src\\views\\loginview.vue"],"sourcesContent":["<template>\r\n  <section class=\"bg-light py-3 py-md-5\">\r\n    <div class=\"container\">\r\n      <div class=\"row justify-content-center\">\r\n        <div class=\"col-12 col-sm-10 col-md-8 col-lg-6 col-xl-5 col-xxl-4\">\r\n          <div class=\"card border border-light-subtle rounded-3 shadow-sm\">\r\n            <div class=\"card-body p-3 p-md-4 p-xl-5\">\r\n              <div class=\"text-center mb-3\">\r\n                <i class=\"bi bi-person-circle\" style=\"font-size: 4rem; color: #007bff;\"></i>\r\n              </div>\r\n              <h2 class=\"fs-6 fw-normal text-center text-secondary mb-4\">Sign in to your account</h2>\r\n\r\n              <!-- Error message display -->\r\n              <transition name=\"fade\">\r\n                <div v-if=\"error\" class=\"alert alert-danger\" role=\"alert\">\r\n                  {{ error }}\r\n                </div>\r\n              </transition>\r\n\r\n              <!-- Login form -->\r\n              <form @submit.prevent=\"handleLogin\">\r\n                <div class=\"row gy-2 overflow-hidden\">\r\n                  <div class=\"col-12\">\r\n                    <div class=\"form-floating mb-3\">\r\n                      <input\r\n                        type=\"email\"\r\n                        class=\"form-control\"\r\n                        v-model=\"email\"\r\n                        id=\"email\"\r\n                        placeholder=\"name@example.com\"\r\n                        required\r\n                      />\r\n                      <label for=\"email\" class=\"form-label\">Email</label>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-12\">\r\n                    <div class=\"form-floating mb-3\">\r\n                      <input\r\n                        type=\"password\"\r\n                        class=\"form-control\"\r\n                        v-model=\"password\"\r\n                        id=\"password\"\r\n                        placeholder=\"Password\"\r\n                        required\r\n                      />\r\n                      <label for=\"password\" class=\"form-label\">Password</label>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-12\">\r\n                    <div class=\"d-flex gap-2 justify-content-between\">\r\n                      <div class=\"form-check\">\r\n                        <input\r\n                          class=\"form-check-input\"\r\n                          type=\"checkbox\"\r\n                          v-model=\"rememberMe\"\r\n                          id=\"rememberMe\"\r\n                        />\r\n                        <label class=\"form-check-label text-secondary\" for=\"rememberMe\">Keep me logged in</label>\r\n                      </div>\r\n                      <a href=\"#!\" class=\"link-primary text-decoration-none\">Forgot password?</a>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-12\">\r\n                    <div class=\"d-grid my-3\">\r\n                      <button class=\"btn btn-primary btn-lg\" type=\"submit\">Log in</button>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-12\">\r\n                    <p class=\"m-0 text-secondary text-center\">Don't have an account? \r\n                      <router-link to=\"/register\" class=\"link-primary text-decoration-none\">Sign up</router-link>\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"LoginComponent\",\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\",\r\n      rememberMe: false,\r\n      error: null,\r\n    };\r\n  },\r\n  methods: {\r\n    async handleLogin() {\r\n        this.error = null;\r\n\r\n        // Check if email and password are filled before sending request\r\n        if (!this.email || !this.password) {\r\n            this.error = \"Please enter both email and password.\";\r\n            return;\r\n        }\r\n\r\n        try {\r\n            // Make the login request\r\n            const response = await axios.post('http://localhost:5555/login', {\r\n                email: this.email,\r\n                password: this.password,\r\n                rememberMe: this.rememberMe\r\n            });\r\n\r\n            // Check if user is verified\r\n            if (response.data.verified) {\r\n                // Redirect to the home page if verified\r\n                this.$router.push(\"/home\");\r\n            } else {\r\n                // If not verified, show error message\r\n                this.error = 'Please verify your Gmail before logging in.';\r\n            }\r\n        } catch (err) {\r\n            // Handle different errors\r\n            if (err.response && err.response.status === 401) {\r\n                this.error = err.response.data.message || \"Invalid email or password.\";\r\n            } else {\r\n                this.error = err.response?.data?.message || \"Login failed. Please try again.\";\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nbody {\r\n  margin: 0;\r\n  padding: 0;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  min-height: 100vh;\r\n  font-family: 'Jost', sans-serif;\r\n  background: linear-gradient(to bottom, #0f0c29, #302b63, #24243e);\r\n}\r\n\r\n.bg-light {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  min-height: 100vh;\r\n  background: transparent;\r\n}\r\n\r\n.card {\r\n  position: relative;\r\n  width: 350px;\r\n  height: 500px;\r\n  border-radius: 10px;\r\n  box-shadow: 5px 20px 50px #000;\r\n  overflow: hidden;\r\n  background: url(\"https://doc-08-2c-docs.googleusercontent.com/docs/securesc/68c90smiglihng9534mvqmq1946dmis5/fo0picsp1nhiucmc0l25s29respgpr4j/1631524275000/03522360960922298374/03522360960922298374/1Sx0jhdpEpnNIydS4rnN4kHSJtU1EyWka?e=view&authuser=0&nonce=gcrocepgbb17m&user=03522