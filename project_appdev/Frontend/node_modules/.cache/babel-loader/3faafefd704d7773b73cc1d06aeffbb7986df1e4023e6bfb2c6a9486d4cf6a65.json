{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      products: [],\n      stats: [{\n        label: 'Satisfied Customers',\n        value: '500+'\n      }, {\n        label: 'Products Available',\n        value: '200+'\n      }, {\n        label: 'Fast Delivery',\n        value: '24/7'\n      }],\n      cards: [{\n        title: 'Free Shipping',\n        description: 'On orders over $50',\n        bgColor: 'primary',\n        icon: 'icon-free-shipping'\n      }, {\n        title: '24/7 Support',\n        description: 'We are here to help',\n        bgColor: 'secondary',\n        icon: 'icon-support'\n      }, {\n        title: 'Best Quality',\n        description: 'Top-notch supplies',\n        bgColor: 'success',\n        icon: 'icon-quality'\n      }]\n    };\n  },\n  created() {\n    this.loadProducts();\n  },\n  methods: {\n    async loadProducts() {\n      try {\n        const response = await axios.get('http://localhost:5555/products');\n        this.products = response.data;\n      } catch (error) {\n        console.error('Failed to load products:', error);\n      }\n    },\n    validateQuantity(product) {\n      if (product.quantity < 1) product.quantity = 1;\n    },\n    addToCart(product) {\n      if (!product.quantity || product.quantity < 1) {\n        product.quantity = 1;\n      }\n      console.log('Adding to cart:', product);\n      // Your cart logic here\n    }\n  }\n};","map":{"version":3,"names":["axios","data","products","stats","label","value","cards","title","description","bgColor","icon","created","loadProducts","methods","response","get","error","console","validateQuantity","product","quantity","addToCart","log"],"sources":["C:\\laragon\\www\\admin_ecommerce\\vue\\src\\views\\home.vue"],"sourcesContent":["<template>\r\n  <section\r\n    style=\"background-image: url('@/assets/images/aa.png'); background-repeat: no-repeat; background-size: cover;\"\r\n  >\r\n    <div class=\"container-lg\">\r\n      <div class=\"row\">\r\n        <div class=\"col-lg-6 pt-5 mt-5\">\r\n          <h2 class=\"display-1 ls-1\">\r\n            <span class=\"fw-bold text-primary\">School</span> Supplies at your\r\n            <span class=\"fw-bold\">Doorsteps</span>\r\n          </h2>\r\n          <p class=\"fs-4\">Everything you need for a successful school year.</p>\r\n          <div class=\"d-flex gap-3\">\r\n            <router-link\r\n              to=\"/product\"\r\n              class=\"btn btn-primary text-uppercase fs-6 rounded-pill px-4 py-3 mt-3\"\r\n              >Start Shopping</router-link\r\n            >\r\n            <router-link\r\n              to=\"/join\"\r\n              class=\"btn btn-dark text-uppercase fs-6 rounded-pill px-4 py-3 mt-3\"\r\n              >Join Now</router-link\r\n            >\r\n          </div>\r\n          <div class=\"row my-5\">\r\n            <div class=\"col\" v-for=\"stat in stats\" :key=\"stat.label\">\r\n              <div class=\"row text-dark\">\r\n                <div class=\"col-auto\">\r\n                  <p class=\"fs-1 fw-bold lh-sm mb-0\">{{ stat.value }}</p>\r\n                </div>\r\n                <div class=\"col\">\r\n                  <p class=\"text-uppercase lh-sm mb-0\">{{ stat.label }}</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div\r\n        class=\"row row-cols-1 row-cols-sm-3 row-cols-lg-3 g-0 justify-content-center\"\r\n      >\r\n        <div v-for=\"card in cards\" :key=\"card.title\" class=\"col\">\r\n          <div :class=\"`card border-0 rounded-0 p-4 text-light bg-${card.bgColor}`\">\r\n            <div class=\"row\">\r\n              <div class=\"col-md-3 text-center\">\r\n                <svg width=\"60\" height=\"60\">\r\n                  <use :xlink:href=\"`#${card.icon}`\"></use>\r\n                </svg>\r\n              </div>\r\n              <div class=\"col-md-9\">\r\n                <div class=\"card-body p-0\">\r\n                  <h5 class=\"text-light\">{{ card.title }}</h5>\r\n                  <p class=\"card-text\">{{ card.description }}</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n\r\n  <section class=\"pb-5\">\r\n    <div class=\"container-lg\">\r\n      <div class=\"row\">\r\n        <div class=\"col-md-12\">\r\n          <div class=\"section-header d-flex flex-wrap justify-content-between my-4\">\r\n            <h2 class=\"section-title\">Best Selling Products</h2>\r\n            <router-link to=\"/product\" class=\"btn btn-primary rounded-1\">View All</router-link>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div class=\"col-md-12\">\r\n          <div\r\n            class=\"product-grid row row-cols-1 row-cols-sm-2 row-cols-md-3 row-cols-lg-3 row-cols-xl-4 row-cols-xxl-5\"\r\n          >\r\n            <div\r\n              v-for=\"product in products.slice(0, 5)\"\r\n              :key=\"product.id\"\r\n              class=\"col\"\r\n            >\r\n              <div\r\n                class=\"product-item\"\r\n                @mouseover=\"hover = true\"\r\n                @mouseleave=\"hover = false\"\r\n              >\r\n                <figure>\r\n                  <router-link\r\n                    :to=\"{ name: 'Product', params: { id: product.id } }\"\r\n                    :title=\"product.name\"\r\n                  >\r\n                    <img\r\n                      :src=\"product.image\"\r\n                      :alt=\"product.name\"\r\n                      class=\"tab-image\"\r\n                      style=\"width: 100%; height: 200px; object-fit: cover;\"\r\n                    />\r\n                  </router-link>\r\n                </figure>\r\n                <div class=\"d-flex flex-column text-center\">\r\n                  <h3 class=\"fs-6 fw-normal\">{{ product.name }}</h3>\r\n                  <div>\r\n                    <span class=\"rating\">\r\n                      <svg\r\n                        width=\"18\"\r\n                        height=\"18\"\r\n                        class=\"text-warning\"\r\n                        v-for=\"i in 4\"\r\n                        :key=\"i\"\r\n                        ><use xlink:href=\"#star-full\"></use></svg\r\n                      >\r\n                      <svg width=\"18\" height=\"18\" class=\"text-warning\">\r\n                        <use xlink:href=\"#star-half\"></use>\r\n                      </svg>\r\n                    </span>\r\n                    <span>(4)</span>\r\n                  </div>\r\n                  <div class=\"d-flex justify-content-center align-items-center gap-2\">\r\n                    <del>${{ product.price + 6 }}</del>\r\n                    <span class=\"text-dark fw-semibold\">${{ product.price }}</span>\r\n                    <span\r\n                      class=\"badge border border-dark-subtle rounded-0 fw-normal px-1 fs-7 lh-1 text-body-tertiary\"\r\n                      >10% OFF</span\r\n                    >\r\n                  </div>\r\n                  <div class=\"button-area p-3 pt-0\">\r\n                    <div class=\"row g-1 mt-2\">\r\n                      <div class=\"col-3\">\r\n                        <input\r\n                          type=\"number\"\r\n                          name=\"quantity\"\r\n                          class=\"form-control border-dark-subtle input-number quantity\"\r\n                          v-model.number=\"product.quantity\"\r\n                          min=\"1\"\r\n                          @input=\"validateQuantity(product)\"\r\n                        />\r\n                      </div>\r\n                      <div class=\"col-9\">\r\n                        <button\r\n                          type=\"button\"\r\n                          class=\"btn btn-primary rounded-1 p-2 fs-7 btn-cart\"\r\n                          @click=\"addToCart(product)\"\r\n                        >\r\n                          <svg width=\"18\" height=\"18\">\r\n                            <use xlink:href=\"#cart\"></use>\r\n                          </svg>\r\n                          Add to Cart\r\n                        </button>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <!-- /product-grid -->\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      products: [],\r\n      stats: [\r\n        { label: 'Satisfied Customers', value: '500+' },\r\n        { label: 'Products Available', value: '200+' },\r\n        { label: 'Fast Delivery', value: '24/7' }\r\n      ],\r\n      cards: [\r\n        {\r\n          title: 'Free Shipping',\r\n          description: 'On orders over $50',\r\n          bgColor: 'primary',\r\n          icon: 'icon-free-shipping'\r\n        },\r\n        {\r\n          title: '24/7 Support',\r\n          description: 'We are here to help',\r\n          bgColor: 'secondary',\r\n          icon: 'icon-support'\r\n        },\r\n        {\r\n          title: 'Best Quality',\r\n          description: 'Top-notch supplies',\r\n          bgColor: 'success',\r\n          icon: 'icon-quality'\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  created() {\r\n    this.loadProducts();\r\n  },\r\n  methods: {\r\n    async loadProducts() {\r\n      try {\r\n        const response = await axios.get('http://localhost:5555/products');\r\n        this.products = response.data;\r\n      } catch (error) {\r\n        console.error('Failed to load products:', error);\r\n      }\r\n    },\r\n    validateQuantity(product) {\r\n      if (product.quantity < 1) product.quantity = 1;\r\n    },\r\n    addToCart(product) {\r\n      if (!product.quantity || product.quantity < 1) {\r\n        product.quantity = 1;\r\n      }\r\n      console.log('Adding to cart:', product);\r\n      // Your cart logic here\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.product-item {\r\n  transition: transform 0.2s ease, box-shadow 0.2s ease;\r\n  border: 1px solid #e1e1e1;\r\n  border-radius: 8px;\r\n}\r\n.product-item:hover {\r\n  transform: translateY(-5px);\r\n  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.2);\r\n}\r\n.product-item figure:hover img {\r\n  opacity: 0.8;\r\n  transition: opacity 0.2s ease;\r\n}\r\n</style>\r\n"],"mappings":"AAsKA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,CACL;QAAEC,KAAK,EAAE,qBAAqB;QAAEC,KAAK,EAAE;MAAO,CAAC,EAC/C;QAAED,KAAK,EAAE,oBAAoB;QAAEC,KAAK,EAAE;MAAO,CAAC,EAC9C;QAAED,KAAK,EAAE,eAAe;QAAEC,KAAK,EAAE;MAAO,EACzC;MACDC,KAAK,EAAE,CACL;QACEC,KAAK,EAAE,eAAe;QACtBC,WAAW,EAAE,oBAAoB;QACjCC,OAAO,EAAE,SAAS;QAClBC,IAAI,EAAE;MACR,CAAC,EACD;QACEH,KAAK,EAAE,cAAc;QACrBC,WAAW,EAAE,qBAAqB;QAClCC,OAAO,EAAE,WAAW;QACpBC,IAAI,EAAE;MACR,CAAC,EACD;QACEH,KAAK,EAAE,cAAc;QACrBC,WAAW,EAAE,oBAAoB;QACjCC,OAAO,EAAE,SAAS;QAClBC,IAAI,EAAE;MACR;IAEJ,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,YAAY,CAAC,CAAC;EACrB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMd,KAAK,CAACe,GAAG,CAAC,gCAAgC,CAAC;QAClE,IAAI,CAACb,QAAO,GAAIY,QAAQ,CAACb,IAAI;MAC/B,EAAE,OAAOe,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;MAClD;IACF,CAAC;IACDE,gBAAgBA,CAACC,OAAO,EAAE;MACxB,IAAIA,OAAO,CAACC,QAAO,GAAI,CAAC,EAAED,OAAO,CAACC,QAAO,GAAI,CAAC;IAChD,CAAC;IACDC,SAASA,CAACF,OAAO,EAAE;MACjB,IAAI,CAACA,OAAO,CAACC,QAAO,IAAKD,OAAO,CAACC,QAAO,GAAI,CAAC,EAAE;QAC7CD,OAAO,CAACC,QAAO,GAAI,CAAC;MACtB;MACAH,OAAO,CAACK,GAAG,CAAC,iBAAiB,EAAEH,OAAO,CAAC;MACvC;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}