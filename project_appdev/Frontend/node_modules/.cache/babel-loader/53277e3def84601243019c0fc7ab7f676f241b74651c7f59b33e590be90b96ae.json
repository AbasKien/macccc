{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      name: '',\n      description: '',\n      price: '',\n      image: null\n    };\n  },\n  methods: {\n    handleImageUpload(event) {\n      this.image = event.target.files[0];\n    },\n    async addProduct() {\n      try {\n        const formData = new FormData();\n        formData.append('name', this.productName);\n        formData.append('description', this.productDescription);\n        formData.append('price', this.productPrice);\n        formData.append('image', this.productImage); // Assuming this.productImage is the file\n\n        const response = await axios.post('http://localhost:5555/admin/add-product', formData);\n        if (response.data.success) {\n          // Redirect to the products section\n          this.$router.push(response.data.redirectTo); // Redirects to '/products'\n        } else {\n          // Handle failure\n          console.error(response.data.message);\n        }\n      } catch (error) {\n        console.error('Error adding product:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","name","description","price","image","methods","handleImageUpload","event","target","files","addProduct","formData","FormData","append","productName","productDescription","productPrice","productImage","response","post","success","$router","push","redirectTo","console","error","message"],"sources":["C:\\laragon\\www\\admin_ecommerce\\vue\\src\\views\\admin.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>Add New Product</h1>\r\n    <form @submit.prevent=\"addProduct\">\r\n      <div>\r\n        <label for=\"name\">Product Name</label>\r\n        <input type=\"text\" v-model=\"name\" id=\"name\" required />\r\n      </div>\r\n      <div>\r\n        <label for=\"description\">Description</label>\r\n        <textarea v-model=\"description\" id=\"description\" required></textarea>\r\n      </div>\r\n      <div>\r\n        <label for=\"price\">Price</label>\r\n        <input type=\"number\" v-model=\"price\" id=\"price\" required />\r\n      </div>\r\n      <div>\r\n        <label for=\"image\">Product Image</label>\r\n        <input type=\"file\" @change=\"handleImageUpload\" required />\r\n      </div>\r\n      <button type=\"submit\">Add Product</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nexport default {\r\n  data() {\r\n    return {\r\n      name: '',\r\n      description: '',\r\n      price: '',\r\n      image: null,\r\n    };\r\n  },\r\n  methods: {\r\n    handleImageUpload(event) {\r\n      this.image = event.target.files[0];\r\n    },\r\n    async addProduct() {\r\n    try {\r\n        const formData = new FormData();\r\n        formData.append('name', this.productName);\r\n        formData.append('description', this.productDescription);\r\n        formData.append('price', this.productPrice);\r\n        formData.append('image', this.productImage);  // Assuming this.productImage is the file\r\n\r\n        const response = await axios.post('http://localhost:5555/admin/add-product', formData);\r\n\r\n        if (response.data.success) {\r\n            // Redirect to the products section\r\n            this.$router.push(response.data.redirectTo);  // Redirects to '/products'\r\n        } else {\r\n            // Handle failure\r\n            console.error(response.data.message);\r\n        }\r\n    } catch (error) {\r\n        console.error('Error adding product:', error);\r\n    }\r\n}\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";AA0BA,OAAOA,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,iBAAiBA,CAACC,KAAK,EAAE;MACvB,IAAI,CAACH,KAAI,GAAIG,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IACpC,CAAC;IACD,MAAMC,UAAUA,CAAA,EAAG;MACnB,IAAI;QACA,MAAMC,QAAO,GAAI,IAAIC,Q