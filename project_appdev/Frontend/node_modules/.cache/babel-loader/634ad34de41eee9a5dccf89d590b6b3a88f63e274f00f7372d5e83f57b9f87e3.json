{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.reduce.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      cartItems: []\n    };\n  },\n  computed: {\n    cartTotal() {\n      return this.cartItems.reduce((total, item) => total + item.price * item.quantity, 0);\n    }\n  },\n  created() {\n    this.loadCart();\n  },\n  methods: {\n    async loadCart() {\n      try {\n        const response = await axios.get(\"http://localhost:5555/addTocart\");\n        this.cartItems = response.data.items;\n      } catch (error) {\n        console.error(\"Failed to load cart items:\", error);\n      }\n    },\n    async removeFromCart(id) {\n      try {\n        await axios.delete(`http://localhost:5555/cart/${id}`);\n        this.loadCart();\n      } catch (error) {\n        console.error(\"Failed to remove item from cart:\", error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","cartItems","computed","cartTotal","reduce","total","item","price","quantity","created","loadCart","methods","response","get","items","error","console","removeFromCart","id","delete"],"sources":["C:\\laragon\\www\\admin_ecommerce\\vue\\src\\views\\cart.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <h1>Your Cart</h1>\r\n      <div v-if=\"cartItems.length > 0\" class=\"cart-container\">\r\n        <div class=\"cart-item\" v-for=\"item in cartItems\" :key=\"item.id\">\r\n          <img :src=\"item.image_url\" alt=\"Product Image\" class=\"cart-image\" />\r\n          <div class=\"cart-info\">\r\n            <h3>{{ item.name }}</h3>\r\n            <p>Price: ${{ item.price }}</p>\r\n            <p>Quantity: {{ item.quantity }}</p>\r\n            <button @click=\"removeFromCart(item.id)\" class=\"remove-btn\">Remove</button>\r\n          </div>\r\n        </div>\r\n        <div class=\"cart-summary\">\r\n          <h2>Total: ${{ cartTotal }}</h2>\r\n          <router-link to=\"/checkout\" class=\"checkout-btn\">Proceed to Checkout</router-link>\r\n        </div>\r\n      </div>\r\n      <p v-else>Your cart is empty!</p>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from \"axios\";\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        cartItems: [],\r\n      };\r\n    },\r\n    computed: {\r\n      cartTotal() {\r\n        return this.cartItems.reduce(\r\n          (total, item) => total + item.price * item.quantity,\r\n          0\r\n        );\r\n      },\r\n    },\r\n    created() {\r\n      this.loadCart();\r\n    },\r\n    methods: {\r\n      async loadCart() {\r\n        try {\r\n          const response = await axios.get(\"http://localhost:5555/addTocart\");\r\n          this.cartItems = response.data.items;\r\n        } catch (error) {\r\n          console.error(\"Failed to load cart items:\", error);\r\n        }\r\n      },\r\n      async removeFromCart(id) {\r\n        try {\r\n          await axios.delete(`http://localhost:5555/cart/${id}`);\r\n          this.loadCart();\r\n        } catch (error) {\r\n          console.error(\"Failed to remove item from cart:\", error);\r\n        }\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .cart-container {\r\n    padding: 10px;\r\n  }\r\n  .cart-item {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-bottom: 15px;\r\n  }\r\n  .cart-image {\r\n    width: 80px;\r\n    height: 80px;\r\n    object-fit: cover;\r\n    margin-right: 15px;\r\n  }\r\n  .cart-info {\r\n    flex-grow: 1;\r\n  }\r\n  .remove-btn {\r\n    background-color: #ff3e3e;\r\n    color: white;\r\n    padding: 5px 10px;\r\n    border: none;\r\n    cursor: pointer;\r\n    border-radius: 5px;\r\n  }\r\n  .cart-summary {\r\n    text-align: right;\r\n  }\r\n  .checkout-btn {\r\n    background-color: #4caf50;\r\n    color: white;\r\n    padding: 10px 20px;\r\n    border: none;\r\n    border-radius: 5px;\r\n    text-decoration: none;\r\n  }