{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport \"core-js/modules/esnext.iterator.reduce.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      products: [],\n      // Array to store products\n      cartItemCount: 0,\n      // Tracks the cart item count\n      loading: true // Loading state\n    };\n  },\n  created() {\n    this.fetchProducts();\n    this.updateCartItemCount(); // Update cart count when the page loads\n  },\n  methods: {\n    // Fetch product data from the API\n    async fetchProducts() {\n      try {\n        const response = await axios.get(\"http://localhost:5555/products\");\n        this.products = response.data.products.map(product => ({\n          ...product,\n          orderQuantity: 1 // Initialize quantity input\n        }));\n      } catch (error) {\n        console.error(\"Error fetching products:\", error);\n      } finally {\n        this.loading = false;\n      }\n    },\n    // Add product to the cart and send to backend\n    async addToCart(product) {\n      // Validate quantity\n      if (product.orderQuantity <= 0 || product.orderQuantity > product.stock) {\n        return;\n      }\n\n      // Get cart from localStorage or create an empty array if not present\n      const cart = JSON.parse(localStorage.getItem(\"cart\")) || [];\n\n      // Check if the product is already in the cart\n      const existingProduct = cart.find(item => item.id === product.id);\n\n      // If product exists, update the quantity\n      if (existingProduct) {\n        existingProduct.quantity += product.orderQuantity;\n      } else {\n        // Add new product to the cart\n        cart.push({\n          id: product.id,\n          name: product.name,\n          price: product.price,\n          image: `http://localhost:5555${product.image_url}`,\n          // Add image URL\n          quantity: product.orderQuantity\n        });\n      }\n\n      // Save updated cart to localStorage\n      localStorage.setItem(\"cart\", JSON.stringify(cart));\n\n      // Update the cart item count\n      this.updateCartItemCount();\n\n      // Send the cart to the backend\n      try {\n        await this.saveCartToBackend(cart);\n      } catch (error) {\n        console.error(\"Error sending cart to the backend:\", error);\n      }\n\n      // Reset the order quantity input to 1\n      product.orderQuantity = 1;\n    },\n    // Update the number of items in the cart (from localStorage)\n    updateCartItemCount() {\n      const cart = JSON.parse(localStorage.getItem(\"cart\")) || [];\n      this.cartItemCount = cart.reduce((total, item) => total + item.quantity, 0);\n    },\n    // Send cart data to the backend\n    async saveCartToBackend(cart) {\n      try {\n        // Assuming the backend endpoint is '/api/cart'\n        const response = await axios.post('http://localhost:5555/cart', {\n          cart\n        });\n        console.log('Cart saved to backend:', response.data);\n      } catch (error) {\n        console.error('Error saving cart to backend:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","products","cartItemCount","loading","created","fetchProducts","updateCartItemCount","methods","response","get","map","product","orderQuantity","error","console","addToCart","stock","cart","JSON","parse","localStorage","getItem","existingProduct","find","item","id","quantity","push","name","price","image","image_url","setItem","stringify","saveCartToBackend","reduce","total","post","log"],"sources":["C:\\laragon\\www\\project_appdev\\Frontend\\src\\views\\product.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-page\">\r\n    <!-- Top Announcement Bar -->\r\n    <div class=\"announcement-bar\">\r\n      ðŸšš 3-days return or refund guarantee.\r\n    </div>\r\n\r\n    <header class=\"header\">\r\n  <div class=\"logo\">Margatheo Store</div>\r\n  <nav class=\"nav\">\r\n    <router-link\r\n      to=\"/home\"\r\n      class=\"nav-link\"\r\n      :class=\"{ active: $route.path === '/home' }\"\r\n    >\r\n      Home\r\n    </router-link>\r\n    <router-link\r\n      to=\"/product\"\r\n      class=\"nav-link\"\r\n      :class=\"{ active: $route.path === '/product' }\"\r\n    >\r\n      Shop\r\n    </router-link>\r\n  </nav>\r\n  <div class=\"user-controls\">\r\n    <!-- Cart Icon -->\r\n    <div class=\"cart-container\">\r\n      <router-link to=\"/cart\" class=\"cart-link\">\r\n        <img\r\n          src=\"https://cdn-icons-png.flaticon.com/512/833/833314.png\"\r\n          alt=\"Cart Icon\"\r\n          class=\"cart-icon\"\r\n        />\r\n        <span class=\"cart-count\" v-if=\"cartItemCount > 0\">{{ cartItemCount }}</span>\r\n      </router-link>\r\n    </div>\r\n  </div>\r\n\r\n</header>\r\n\r\n\r\n    <!-- Products Section -->\r\n    <section class=\"products\">\r\n      <div class=\"content\">\r\n        <h1>Shop Now for Premium School Supplies</h1>\r\n        <p class=\"description\">\r\n          Explore a wide variety of school supplies designed to help you succeed\r\n          in style.\r\n        </p>\r\n        <div v-if=\"loading\" class=\"loading\">Loading products...</div>\r\n        <div v-else-if=\"products.length > 0\" class=\"product-grid\">\r\n          <div\r\n            class=\"product-card\"\r\n            v-for=\"product in products\"\r\n            :key=\"product.id\"\r\n          >\r\n            <img\r\n              :src=\"`http://localhost:5555${product.image_url}`\"\r\n              :alt=\"product.name\"\r\n              class=\"product-image\"\r\n            />\r\n            <h2 class=\"product-name\">{{ product.name }}</h2>\r\n            <p class=\"product-price\">â‚±{{ product.price }}</p>\r\n            <button\r\n              @click=\"addToCart(product)\"\r\n              class=\"add-to-cart\"\r\n              :disabled=\"product.stock === 0\"\r\n            >\r\n              {{ product.stock === 0 ? \"Out of Stock\" : \"Add to Cart\" }}\r\n            </button>\r\n          </div>\r\n        </div>\r\n        <p v-else>No products available at the moment.</p>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Footer -->\r\n    <footer class=\"footer\">\r\n      <p>&copy; 2024 Margatheo Store. All Rights Reserved.</p>\r\n    </footer>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      products: [], // Array to store products\r\n      cartItemCount: 0, // Tracks the cart item count\r\n      loading: true, // Loading state\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchProducts();\r\n    this.updateCartItemCount(); // Update cart count when the page loads\r\n  },\r\n  methods: {\r\n    // Fetch product data from the API\r\n    async fetchProducts() {\r\n      try {\r\n        const response = await axios.get(\"http://localhost:5555/products\");\r\n        this.products = response.data.products.map((product) => ({\r\n          ...product,\r\n          orderQuantity: 1, // Initialize quantity input\r\n        }));\r\n      } catch (error) {\r\n        console.error(\"Error fetching products:\", error);\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n\r\n    // Add product to the cart and send to backend\r\n    async addToCart(product) {\r\n      // Validate quantity\r\n      if (product.orderQuantity <= 0 || product.orderQuantity > product.stock) {\r\n        return;\r\n      }\r\n\r\n      // Get cart from localStorage or create an empty array if not present\r\n      const cart = JSON.parse(localStorage.getItem(\"cart\")) || [];\r\n\r\n      // Check if the product is already in the cart\r\n      const existingProduct = cart.find((item) => item.id === product.id);\r\n\r\n      // If product exists, update the quantity\r\n      if (existingProduct) {\r\n        existingProduct.quantity += product.orderQuantity;\r\n      } else {\r\n        // Add new product to the cart\r\n        cart.push({\r\n          id: product.id,\r\n          name: product.name,\r\n          price: product.price,\r\n          image: `http://localhost:5555${product.image_url}`, // Add image URL\r\n          quantity: product.orderQuantity,\r\n        });\r\n      }\r\n\r\n      // Save updated cart to localStorage\r\n      localStorage.setItem(\"cart\", JSON.stringify(cart));\r\n\r\n      // Update the cart item count\r\n      this.updateCartItemCount();\r\n\r\n      // Send the cart to the backend\r\n      try {\r\n        await this.saveCartToBackend(cart);\r\n      } catch (error) {\r\n        console.error(\"Error sending cart to the backend:\", error);\r\n      }\r\n\r\n      // Reset the order quantity input to 1\r\n      product.orderQuantity = 1;\r\n    },\r\n\r\n    // Update the number of items in the cart (from localStorage)\r\n    updateCartItemCount() {\r\n      const cart = JSON.parse(localStorage.getItem(\"cart\")) || [];\r\n      this.cartItemCount = cart.reduce((total, item) => total + item.quantity, 0);\r\n    },\r\n\r\n    // Send cart data to the backend\r\n    async saveCartToBackend(cart) {\r\n      try {\r\n        // Assuming the backend endpoint is '/api/cart'\r\n        const response = await axios.post('http://localhost:5555/cart', { cart });\r\n        console.log('Cart saved to backend:', response.data);\r\n      } catch (error) {\r\n        console.error('Error saving cart to backend:', error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n/* Cart Page Styling */\r\n.cart-page {\r\n  max-width: 1200px;\r\n  margin: 50px auto;\r\n  padding: 20px;\r\n  background-color: #fff;\r\n  border-radius: 8px;\r\n  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.cart-title {\r\n  font-size: 28px;\r\n  font-weight: bold;\r\n  margin-bottom: 20px;\r\n  color: #333;\r\n  text-align: center;\r\n}\r\n\r\n.cart-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.cart-table th,\r\n.cart-table td {\r\n  padding: 15px;\r\n  text-align: center;\r\n  border-bottom: 1px solid #eee;\r\n}\r\n\r\n.cart-table th {\r\n  background-color: #f4511e;\r\n  color: white;\r\n  font-size: 16px;\r\n  text-transform: uppercase;\r\n}\r\n\r\n.cart-table td img {\r\n  width: 80px;\r\n  height: auto;\r\n  border-radius: 5px;\r\n}\r\n\r\n.cart-table .product-name {\r\n  font-weight: bold;\r\n  color: #333;\r\n}\r\n\r\n.cart-table .product-price {\r\n  color: #555;\r\n}\r\n\r\n.cart-table .quantity-input {\r\n  width: 50px;\r\n  text-align: center;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  padding: 5px;\r\n}\r\n\r\n.cart-table .remove-btn {\r\n  background-color: #d73e15;\r\n  color: white;\r\n  border: none;\r\n  padding: 5px 10px;\r\n  cursor: pointer;\r\n  font-size: 14px;\r\n  border-radius: 5px;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.cart-table .remove-btn:hover {\r\n  background-color: #c13010;\r\n}\r\n\r\n.cart-summary {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 20px;\r\n  background-color: #f9f9f9;\r\n  border-radius: 8px;\r\n  box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.cart-summary .total {\r\n  font-size: 20px;\r\n  font-weight: bold;\r\n  color: #333;\r\n}\r\n\r\n.cart-summary .checkout-btn {\r\n  background-color: #f4511e;\r\n  color: white;\r\n  border: none;\r\n  padding: 10px 20px;\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.cart-summary .checkout-btn:hover {\r\n  background-color: #d73e15;\r\n}\r\n\r\n/* Empty Cart Styling */\r\n.empty-cart {\r\n  text-align: center;\r\n  font-size: 18px;\r\n  color: #555;\r\n  padding: 50px;\r\n}\r\n\r\n.empty-cart img {\r\n  width: 150px;\r\n  height: auto;\r\n  margin-bottom: 20px;\r\n}\r\n</style>\r\n"],"mappings":";;;;;AAsFA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MAAE;MACdC,aAAa,EAAE,CAAC;MAAE;MAClBC,OAAO,EAAE,IAAI,CAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;IACpB,IAAI,CAACC,mBAAmB,CAAC,CAAC,EAAE;EAC9B,CAAC;EACDC,OAAO,EAAE;IACP;IACA,MAAMF,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMT,KAAK,CAACU,GAAG,CAAC,gCAAgC,CAAC;QAClE,IAAI,CAACR,QAAO,GAAIO,QAAQ,CAACR,IAAI,CAACC,QAAQ,CAACS,GAAG,CAAEC,OAAO,KAAM;UACvD,GAAGA,OAAO;UACVC,aAAa,EAAE,CAAC,CAAE;QACpB,CAAC,CAAC,CAAC;MACL,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;MAClD,UAAU;QACR,IAAI,CAACV,OAAM,GAAI,KAAK;MACtB;IACF,CAAC;IAED;IACA,MAAMY,SAASA,CAACJ,OAAO,EAAE;MACvB;MACA,IAAIA,OAAO,CAACC,aAAY,IAAK,KAAKD,OAAO,CAACC,aAAY,GAAID,OAAO,CAACK,KAAK,EAAE;QACvE;MACF;;MAEA;MACA,MAAMC,IAAG,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,KAAK,EAAE;;MAE3D;MACA,MAAMC,eAAc,GAAIL,IAAI,CAACM,IAAI,CAAEC,IAAI,IAAKA,IAAI,CAACC,EAAC,KAAMd,OAAO,CAACc,EAAE,CAAC;;MAEnE;MACA,IAAIH,eAAe,EAAE;QACnBA,eAAe,CAACI,QAAO,IAAKf,OAAO,CAACC,aAAa;MACnD,OAAO;QACL;QACAK,IAAI,CAACU,IAAI,CAAC;UACRF,EAAE,EAAEd,OAAO,CAACc,EAAE;UACdG,IAAI,EAAEjB,OAAO,CAACiB,IAAI;UAClBC,KAAK,EAAElB,OAAO,CAACkB,KAAK;UACpBC,KAAK,EAAE,wBAAwBnB,OAAO,CAACoB,SAAS,EAAE;UAAE;UACpDL,QAAQ,EAAEf,OAAO,CAACC;QACpB,CAAC,CAAC;MACJ;;MAEA;MACAQ,YAAY,CAACY,OAAO,CAAC,MAAM,EAAEd,IAAI,CAACe,SAAS,CAAChB,IAAI,CAAC,CAAC;;MAElD;MACA,IAAI,CAACX,mBAAmB,CAAC,CAAC;;MAE1B;MACA,IAAI;QACF,MAAM,IAAI,CAAC4B,iBAAiB,CAACjB,IAAI,CAAC;MACpC,EAAE,OAAOJ,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,oCAAoC,EAAEA,KAAK,CAAC;MAC5D;;MAEA;MACAF,OAAO,CAACC,aAAY,GAAI,CAAC;IAC3B,CAAC;IAED;IACAN,mBAAmBA,CAAA,EAAG;MACpB,MAAMW,IAAG,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,KAAK,EAAE;MAC3D,IAAI,CAACnB,aAAY,GAAIe,IAAI,CAACkB,MAAM,CAAC,CAACC,KAAK,EAAEZ,IAAI,KAAKY,KAAI,GAAIZ,IAAI,CAACE,QAAQ,EAAE,CAAC,CAAC;IAC7E,CAAC;IAED;IACA,MAAMQ,iBAAiBA,CAACjB,IAAI,EAAE;MAC