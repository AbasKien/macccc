{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"LoginComponent\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      rememberMe: false,\n      error: null\n    };\n  },\n  methods: {\n    async handleLogin() {\n      this.error = null;\n      if (!this.email || !this.password) {\n        this.error = \"Please enter both email and password.\";\n        return;\n      }\n      try {\n        const response = await axios.post(\"http://localhost:5555/login\", {\n          email: this.email,\n          password: this.password,\n          rememberMe: this.rememberMe\n        });\n        if (response.data.verified) {\n          this.$router.push(\"/home\");\n        } else {\n          this.error = \"Please verify your Gmail before logging in.\";\n        }\n      } catch (err) {\n        this.error = err.response?.data?.message || \"Login failed. Please try again.\";\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","rememberMe","error","methods","handleLogin","response","post","verified","$router","push","err","message"],"sources":["C:\\laragon\\www\\project_appdev\\Frontend\\src\\views\\loginview.vue"],"sourcesContent":["<template>\r\n  <section class=\"login-section bg-light\">\r\n    <div class=\"container py-5\">\r\n      <div class=\"row justify-content-center\">\r\n        <div class=\"col-12 col-md-8 col-lg-6\">\r\n          <div class=\"card border-0 shadow-lg rounded-4\">\r\n            <div class=\"row g-0\">\r\n              <!-- Left Image Section -->\r\n              <div class=\"col-12 col-md-5 d-none d-md-block\">\r\n                <img\r\n                  src=\"https://i.pinimg.com/originals/1f/85/3e/1f853ed9cbe6c49b9e0127fe20803c86.jpg?text=School+Supplies\"\r\n                  alt=\"School Supplies\"\r\n                  class=\"img-fluid h-100 rounded-start\"\r\n                  style=\"object-fit: cover;\"\r\n                />\r\n              </div>\r\n\r\n              <!-- Right Form Section -->\r\n              <div class=\"col-12 col-md-7\">\r\n                <div class=\"card-body p-4 p-md-5\">\r\n                  <!-- Logo or Illustration -->\r\n                  <div class=\"text-center mb-4\">\r\n                    <img\r\n                      src=\"https://i.pinimg.com/originals/1f/85/3e/1f853ed9cbe6c49b9e0127fe20803c86.jpg?text=Logo\"\r\n                      alt=\"Logo\"\r\n                      class=\"mb-2\"\r\n                      style=\"width: 80px;\"\r\n                    />\r\n                    <h1 class=\"fs-4 fw-semibold text-primary\">\r\n                      Welcome Back!\r\n                    </h1>\r\n                    <p class=\"text-secondary\">\r\n                      Login to shop high-quality school supplies at great prices!\r\n                    </p>\r\n                  </div>\r\n\r\n                  <!-- Error Message -->\r\n                  <transition name=\"fade\">\r\n                    <div v-if=\"error\" class=\"alert alert-danger text-center py-2\" role=\"alert\">\r\n                      {{ error }}\r\n                    </div>\r\n                  </transition>\r\n\r\n                  <!-- Login Form -->\r\n                  <form @submit.prevent=\"handleLogin\">\r\n                    <!-- Email -->\r\n                    <div class=\"form-floating mb-3\">\r\n                      <input\r\n                        type=\"email\"\r\n                        class=\"form-control\"\r\n                        v-model=\"email\"\r\n                        id=\"email\"\r\n                        placeholder=\"name@example.com\"\r\n                        required\r\n                      />\r\n                      <label for=\"email\">Email</label>\r\n                    </div>\r\n\r\n                    <!-- Password -->\r\n                    <div class=\"form-floating mb-3\">\r\n                      <input\r\n                        type=\"password\"\r\n                        class=\"form-control\"\r\n                        v-model=\"password\"\r\n                        id=\"password\"\r\n                        placeholder=\"Password\"\r\n                        required\r\n                      />\r\n                      <label for=\"password\">Password</label>\r\n                    </div>\r\n\r\n                    <!-- Keep Me Logged In -->\r\n                    <div class=\"d-flex justify-content-between align-items-center mb-3\">\r\n                      <div class=\"form-check\">\r\n                        <input\r\n                          class=\"form-check-input\"\r\n                          type=\"checkbox\"\r\n                          v-model=\"rememberMe\"\r\n                          id=\"rememberMe\"\r\n                        />\r\n                        <label class=\"form-check-label text-secondary\" for=\"rememberMe\">Keep me logged in</label>\r\n                      </div>\r\n                      <a href=\"#!\" class=\"text-decoration-none text-primary fw-semibold\">Forgot password?</a>\r\n                    </div>\r\n\r\n                    <!-- Submit Button -->\r\n                    <div class=\"d-grid mb-3\">\r\n                      <button class=\"btn btn-primary btn-lg\" type=\"submit\">Log In</button>\r\n                    </div>\r\n\r\n                    <!-- Register Link -->\r\n                    <p class=\"text-center text-secondary mt-3\">\r\n                      Don't have an account?\r\n                      <router-link to=\"/register\" class=\"text-primary fw-semibold text-decoration-none\">Sign up</router-link>\r\n                    </p>\r\n                  </form>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Additional Call-to-Action Below Card -->\r\n          <div class=\"text-center mt-4\">\r\n            <p class=\"text-secondary\">\r\n              Looking for amazing school supplies? Check out our \r\n              <router-link to=\"/products\" class=\"text-primary fw-semibold\">Shop Now</router-link>\r\n            </p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"LoginComponent\",\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\",\r\n      rememberMe: false,\r\n      error: null,\r\n    };\r\n  },\r\n  methods: {\r\n    async handleLogin() {\r\n      this.error = null;\r\n      if (!this.email || !this.password) {\r\n        this.error = \"Please enter both email and password.\";\r\n        return;\r\n      }\r\n      try {\r\n        const response = await axios.post(\"http://localhost:5555/login\", {\r\n          email: this.email,\r\n          password: this.password,\r\n          rememberMe: this.rememberMe,\r\n        });\r\n        if (response.data.verified) {\r\n          this.$router.push(\"/home\");\r\n        } else {\r\n          this.error = \"Please verify your Gmail before logging in.\";\r\n        }\r\n      } catch (err) {\r\n        this.error =\r\n          err.response?.data?.message || \"Login failed. Please try again.\";\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Section Background */\r\n.login-section {\r\n  background: linear-gradient(to bottom, #f3f4f6, #ffffff);\r\n  min-height: 100vh;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n/* Card Styling */\r\n.card {\r\n  overflow: hidden;\r\n}\r\n\r\n.card-body {\r\n  color: #333;\r\n}\r\n\r\n.img-fluid {\r\n  border-top-left-radius: 0.5rem;\r\n  border-bottom-left-radius: 0.5rem;\r\n}\r\n\r\n/* Buttons */\r\n.btn-primary {\r\n  background: linear-gradient(to right, #6d44b8, #573b8a);\r\n  border: none;\r\n  border-radius: 8px;\r\n  transition: background 0.3s ease-in-out, transform 0.2s ease;\r\n}\r\n\r\n.btn-primary:hover {\r\n  background: linear-gradient(to right, #573b8a, #6d44b8);\r\n  transform: translateY(-2px);\r\n}\r\n\r\n/* Text Links */\r\n.text-primary {\r\n  color: #573b8a;\r\n  font-weight: bold;\r\n  text-decoration: none;\r\n  transition: color 0.3s ease;\r\n}\r\n\r\n.text-primary:hover {\r\n  color: #6d44b8;\r\n  text-decoration: underline;\r\n}\r\n\r\n/* Alerts */\r\n.alert {\r\n  font-size: 0.9rem;\r\n  font-weight: bold;\r\n  background: rgba(255, 0, 0, 0.1);\r\n  color: #ff0000;\r\n  border: 1px solid rgba(255, 0, 0, 0.2);\r\n  border-radius: 8px;\r\n  padding: 10px;\r\n}\r\n\r\n/* Responsive Adjustments */\r\n@media (max-width: 768px) {\r\n  .card img {\r\n    display: none;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AAmHA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,KAAK;MACjBC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI,CAACF,KAAI,GAAI,IAAI;MACjB,IAAI,CAAC,IAAI,CAACH,KAAI,IAAK,CAAC,IAAI,CAACC,QAAQ,EAAE;QACjC,IAAI,CAACE,KAAI,GAAI,uCAAuC;QACpD;MACF;MACA,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMT,KAAK,CAACU,IAAI,CAAC,6BAA6B,EAAE;UAC/DP,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,UAAU,EAAE,IAAI,CAACA;QACnB,CAAC,CAAC;QACF,IAAII,QAAQ,CAACP,IAAI,CAACS,QAAQ,EAAE;UAC1B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,OAAO,CAAC;QAC5B,OAAO;UACL,IAAI,CAACP,KAAI,GAAI,6CAA6C;QAC5D;MACF,EAAE,OAAOQ,GAAG,EAAE;QACZ,IAAI,CAACR,KAAI,GACPQ,GAAG,CAACL,QAAQ,EAAEP,IAAI,EAAEa,OAAM,IAAK,iCAAiC;MACpE;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}