{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: \"RegisterComponent\",\n  data() {\n    return {\n      name: '',\n      email: '',\n      password: '',\n      error: null\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      this.error = null; // Clear previous errors\n      try {\n        // Send the form data to the server\n        const response = await axios.post('http://localhost:5555/register', {\n          name: this.name,\n          email: this.email,\n          password: this.password\n        });\n\n        // If registration was successful, show success message or redirect\n        if (response.status === 200) {\n          this.$router.push(\"/\"); // Redirect to login page\n        }\n      } catch (err) {\n        // Handle error responses from the server\n        if (err.response && err.response.data.message) {\n          this.error = err.response.data.message || 'Registration failed. Please try again.';\n        } else {\n          this.error = 'An error occurred. Please try again later.';\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","error","methods","handleSubmit","response","post","status","$router","push","err","message"],"sources":["C:\\laragon\\www\\project_appdev\\Frontend\\src\\views\\register.vue"],"sourcesContent":["<template>\r\n    <section class=\"bg-light py-3 py-md-5\">\r\n        <div class=\"container\">\r\n            <div class=\"row justify-content-center\">\r\n                <div class=\"col-12 col-sm-10 col-md-8 col-lg-6 col-xl-5 col-xxl-4\">\r\n                    <div class=\"card border border-light-subtle rounded-3 shadow-sm\">\r\n                        <div class=\"card-body p-3 p-md-4 p-xl-5\">\r\n                            <div class=\"text-center mb-3\">\r\n                                <i class=\"bi bi-person-plus\" style=\"font-size: 4rem; color: #007bff;\"></i>\r\n                            </div>\r\n   <!-- Logo -->\r\n   <div class=\"text-center mb-4\">\r\n          <img\r\n            src=\"https://media.istockphoto.com/vectors/user-member-vector-icon-for-ui-user-interface-or-profile-face-avatar-vector-id1130884625?k=6&m=1130884625&s=170667a&w=0&h=b4ICEL-2imqnsT-m2tYGxZdxlgD1yKxmoDA-PmPc2-A=\"\r\n            alt=\"Logo\"\r\n            class=\"logo mb-2\"\r\n          />\r\n          <h2 class=\"fs-4 fw-bold text-primary\">Welcome Back!</h2>\r\n          <p class=\"text-muted small\">Sign up to shop quality school supplies.</p>\r\n        </div>\r\n                            <!-- Display error message if exists -->\r\n                            <div v-if=\"error\" class=\"alert alert-danger\" role=\"alert\">\r\n                                {{ error }}\r\n                            </div>\r\n\r\n                            <!-- Registration form -->\r\n                            <form @submit.prevent=\"handleSubmit\">\r\n                                <div class=\"row gy-2 overflow-hidden\">\r\n                                    <!-- Name Field -->\r\n                                    <div class=\"col-12\">\r\n                                        <div class=\"form-floating mb-3\">\r\n                                            <input \r\n                                                type=\"text\" \r\n                                                class=\"form-control\" \r\n                                                v-model=\"name\" \r\n                                                id=\"name\" \r\n                                                placeholder=\"Full Name\" \r\n                                                required\r\n                                            />\r\n                                            <label for=\"name\" class=\"form-label\">Full Name</label>\r\n                                        </div>\r\n                                    </div>\r\n\r\n                                    <!-- Email Field -->\r\n                                    <div class=\"col-12\">\r\n                                        <div class=\"form-floating mb-3\">\r\n                                            <input \r\n                                                type=\"email\" \r\n                                                class=\"form-control\" \r\n                                                v-model=\"email\" \r\n                                                id=\"email\" \r\n                                                placeholder=\"name@example.com\" \r\n                                                required\r\n                                            />\r\n                                            <label for=\"email\" class=\"form-label\">Email</label>\r\n                                        </div>\r\n                                    </div>\r\n\r\n                                    <!-- Password Field -->\r\n                                    <div class=\"col-12\">\r\n                                        <div class=\"form-floating mb-3\">\r\n                                            <input \r\n                                                type=\"password\" \r\n                                                class=\"form-control\" \r\n                                                v-model=\"password\" \r\n                                                id=\"password\" \r\n                                                placeholder=\"Password\" \r\n                                                required\r\n                                            />\r\n                                            <label for=\"password\" class=\"form-label\">Password</label>\r\n                                        </div>\r\n                                    </div>\r\n\r\n                                    <!-- Submit Button -->\r\n                                    <div class=\"col-12\">\r\n                                        <div class=\"d-grid my-3\">\r\n                                            <button class=\"btn btn-primary btn-lg\" type=\"submit\">Sign Up</button>\r\n                                        </div>\r\n                                    </div>\r\n\r\n                                    <!-- Already have an account -->\r\n                                    <div class=\"col-12\">\r\n                                        <p class=\"m-0 text-secondary text-center\">\r\n                                            Already have an account? \r\n                                            <router-link to=\"/\" class=\"link-primary text-decoration-none\">Log in</router-link>\r\n                                        </p>\r\n                                    </div>\r\n                                </div>\r\n                            </form>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    name: \"RegisterComponent\",\r\n    data() {\r\n        return {\r\n            name: '',\r\n            email: '',\r\n            password: '',\r\n            error: null,\r\n        };\r\n    },\r\n    methods: {\r\n        async handleSubmit() {\r\n    this.error = null; // Clear previous errors\r\n    try {\r\n        // Send the form data to the server\r\n        const response = await axios.post('http://localhost:5555/register', {\r\n            name: this.name,\r\n            email: this.email,\r\n            password: this.password\r\n        });\r\n\r\n        // If registration was successful, show success message or redirect\r\n        if (response.status === 200) {\r\n            this.$router.push(\"/\"); // Redirect to login page\r\n        }\r\n\r\n    } catch (err) {\r\n        // Handle error responses from the server\r\n        if (err.response && err.response.data.message) {\r\n            this.error = err.response.data.message || 'Registration failed. Please try again.';\r\n        } else {\r\n            this.error = 'An error occurred. Please try again later.';\r\n        }\r\n    }\r\n}\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nbody {\r\n    margin: 0;\r\n    padding: 0;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    min-height: 100vh;\r\n    font-family: 'Jost', sans-serif;\r\n    background: linear-gradient(to bottom, #0f0c29, #302b63, #24243e);\r\n}\r\n\r\n.bg-light {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    min-height: 100vh;\r\n    background: transparent;\r\n}\r\n\r\n.card {\r\n    border: none;\r\n    border-radius: 10px;\r\n    box-shadow: 5px 20px 50px rgba(0, 0, 0, 0.6);\r\n    overflow: hidden;\r\n    background: linear-gradient(to bottom right, rgba(255, 255, 255, 0.8), rgba(255, 255, 255, 0.5));\r\n    backdrop-filter: blur(10px);\r\n}\r\n\r\n.card-body {\r\n    padding: 30px 20px;\r\n    color: #333;\r\n    text-align: center;\r\n}\r\n\r\n.fs-6 {\r\n    font-size: 1.2rem;\r\n    font-weight: 600;\r\n    color: #24243e;\r\n}\r\n\r\n.form-control {\r\n    border: 1px solid rgba(255, 255, 255, 0.3);\r\n    background-color: rgba(255, 255, 255, 0.8);\r\n    border-radius: 8px;\r\n    font-size: 1rem;\r\n    padding: 0.75rem;\r\n    color: #24243e;\r\n    transition: border-color 0.3s, background-color 0.3s;\r\n}\r\n\r\n.form-control:focus {\r\n    border-color: #007bff;\r\n    background-color: rgba(255, 255, 255, 1);\r\n    box-shadow: 0 0 5px rgba(0, 123, 255, 0.5);\r\n}\r\n\r\n.btn-primary {\r\n    background: linear-gradient(to right, #6d44b8, #573b8a);\r\n    border: none;\r\n    color: #fff;\r\n    font-size: 1.1rem;\r\n    font-weight: bold;\r\n    padding: 0.75rem;\r\n    border-radius: 8px;\r\n    transition: background 0.3s ease-in-out, transform 0.2s ease;\r\n}\r\n\r\n.btn-primary:hover {\r\n    background: linear-gradient(to right, #573b8a, #6d44b8);\r\n    transform: translateY(-2px);\r\n}\r\n\r\n.link-primary {\r\n    color: #573b8a;\r\n    font-weight: bold;\r\n    text-decoration: none;\r\n    transition: color 0.3s ease;\r\n}\r\n\r\n.link-primary:hover {\r\n    color: #6d44b8;\r\n    text-decoration: underline;\r\n}\r\n\r\n.alert {\r\n    font-size: 0.9rem;\r\n    font-weight: bold;\r\n    background: rgba(255, 0, 0, 0.1);\r\n    color: #ff0000;\r\n    border: 1px solid rgba(255, 0, 0, 0.2);\r\n    border-radius: 8px;\r\n    padding: 10px;\r\n}\r\n.logo {\r\n  width: 60px;\r\n  height: 60px;\r\n  object-fit: cover;\r\n  border-radius: 50%;\r\n}\r\n\r\n</style>\r\n"],"mappings":";AAkGA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,mBAAmB;EACzBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHD,IAAI,EAAE,EAAE;MACRE,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE;IACX,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,YAAYA,CAAA,EAAG;MACzB,IAAI,CAACF,KAAI,GAAI,IAAI,EAAE;MACnB,IAAI;QACA;QACA,MAAMG,QAAO,GAAI,MAAMR,KAAK,CAACS,IAAI,CAAC,gCAAgC,EAAE;UAChER,IAAI,EAAE,IAAI,CAACA,IAAI;UACfE,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACnB,CAAC,CAAC;;QAEF;QACA,IAAII,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UACzB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC,EAAE;QAC5B;MAEJ,EAAE,OAAOC,GAAG,EAAE;QACV;QACA,IAAIA,GAAG,CAACL,QAAO,IAAKK,GAAG,CAACL,QAAQ,CAACN,IAAI,CAACY,OAAO,EAAE;UAC3C,IAAI,CAACT,KAAI,GAAIQ,GAAG,CAACL,QAAQ,CAACN,IAAI,CAACY,OAAM,IAAK,wCAAwC;QACtF,OAAO;UACH,IAAI,CAACT,KAAI,GAAI,4CAA4C;QAC7D;MACJ;IACJ;EACI;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}