{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"LoginComponent\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      error: null\n    };\n  },\n  methods: {\n    async handleLogin() {\n      this.error = null;\n      try {\n        const response = await axios.post('http://localhost:5555/login', {\n          email: this.email,\n          password: this.password\n        });\n        if (response.data.verified) {\n          this.$router.push(\"/home\");\n        }\n      } catch (err) {\n        if (err.response && err.response.status === 401 && err.response.data.message === 'Please verify your email before logging in.') {\n          this.error = 'Please verify your email before logging in.';\n        } else {\n          this.error = err.response?.data?.message || \"Login failed. Please try again.\";\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","error","methods","handleLogin","response","post","verified","$router","push","err","status","message"],"sources":["C:\\laragon\\www\\admin_ecommerce\\vue\\src\\App.vue"],"sourcesContent":["<template>\n  <section class=\"login-section\">\n    <div class=\"container\">\n      <div class=\"row justify-content-center\">\n        <div class=\"col-12 col-sm-10 col-md-8 col-lg-6 col-xl-5 col-xxl-4\">\n          <div class=\"card login-card\">\n            <div class=\"card-body\">\n              <div class=\"text-center mb-3\">\n                <i class=\"bi bi-person-circle login-icon\"></i>\n              </div>\n              <h2 class=\"login-heading\">Sign in to your account</h2>\n\n              <!-- Display error message if it exists -->\n              <div v-if=\"error\" class=\"alert alert-danger\" role=\"alert\">\n                {{ error }}\n              </div>\n\n              <!-- Login form -->\n              <form @submit.prevent=\"handleLogin\">\n                <div class=\"form-floating mb-3\">\n                  <input\n                    type=\"email\"\n                    class=\"form-control\"\n                    v-model=\"email\"\n                    id=\"email\"\n                    placeholder=\"name@example.com\"\n                    required\n                  />\n                  <label for=\"email\">Email</label>\n                </div>\n                <div class=\"form-floating mb-3\">\n                  <input\n                    type=\"password\"\n                    class=\"form-control\"\n                    v-model=\"password\"\n                    id=\"password\"\n                    placeholder=\"Password\"\n                    required\n                  />\n                  <label for=\"password\">Password</label>\n                </div>\n                <div class=\"d-grid my-3\">\n                  <button class=\"btn login-btn\" type=\"submit\">Log in</button>\n                </div>\n                <p class=\"text-secondary text-center\">\n                  Don't have an account?\n                  <router-link to=\"/register\" class=\"link-primary\">Sign up</router-link>\n                </p>\n              </form>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"LoginComponent\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      error: null,\n    };\n  },\n  methods: {\n    async handleLogin() {\n      this.error = null;\n      try {\n        const response = await axios.post('http://localhost:5555/login', {\n          email: this.email,\n          password: this.password\n        });\n        if (response.data.verified) {\n          this.$router.push(\"/home\");\n        }\n      } catch (err) {\n        if (err.response && err.response.status === 401 && err.response.data.message === 'Please verify your email before logging in.') {\n          this.error = 'Please verify your email before logging in.';\n        } else {\n          this.error = err.response?.data?.message || \"Login failed. Please try again.\";\n        }\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.login-section {\n  background-color: #f3f4f6;\n  padding: 80px 0;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  min-height: 100vh;\n}\n\n.login-card {\n  border-radius: 12px;\n  padding: 2rem;\n  background: #ffffff;\n  border: none;\n  box-shadow: 0 8px 20px rgba(0, 0, 0, 0.15);\n}\n\n.card-body {\n  padding: 2rem;\n}\n\n.login-icon {\n  font-size: 4rem;\n  color: #007bff;\n}\n\n.login-heading {\n  font-size: 1.5rem;\n  font-weight: 600;\n  color: #495057;\n  text-align: center;\n  margin-bottom: 1.5rem;\n}\n\n.form-floating .form-control {\n  padding: 0.25rem 1rem;\n  font-size: 1rem;\n  border-radius: 8px;\n  border: 1px solid #ced4da;\n  background-color: #f8f9fa;\n}\n\n.form-floating .form-control:focus {\n  border-color: #007bff;\n  box-shadow: 0 0 0 0.2rem rgba(0, 123, 255, 0.25);\n}\n\n.login-btn {\n  font-size: 1.1rem;\n  padding: 0.25rem;\n  background-color: #007bff;\n  border: none;\n  border-radius: 8px;\n  font-weight: 600;\n  transition: background-color 0.3s ease;\n}\n\n.login-btn:hover {\n  background-color: #0056b3;\n}\n\n.alert {\n  font-size: 0.875rem;\n  margin-top: 1rem;\n  border-radius: 8px;\n}\n\n.link-primary {\n  color: #007bff;\n  font-weight: 500;\n  transition: color 0.3s ease;\n}\n\n.link-primary:hover {\n  color: #0056b3;\n  text-decoration: underline;\n}\n\n.text-secondary {\n  color: #6c757d;\n}\n</style>\n"],"mappings":";AA0DA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI,CAACF,KAAI,GAAI,IAAI;MACjB,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMR,KAAK,CAACS,IAAI,CAAC,6BAA6B,EAAE;UAC/DN,KAAK,EAAE,IAAI,CA