{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      activeTab: \"details\",\n      // Default active tab\n      user: {\n        name: \"\",\n        email: \"\",\n        phone: \"\",\n        address: \"\"\n      },\n      orders: [{\n        id: \"ORD12345\",\n        date: \"2024-11-20\",\n        total: 149.99\n      }, {\n        id: \"ORD67890\",\n        date: \"2024-10-15\",\n        total: 89.99\n      }],\n      settings: {\n        newPassword: \"\",\n        notifications: true\n      }\n    };\n  },\n  mounted() {\n    // Fetch user data from localStorage\n    const userData = JSON.parse(localStorage.getItem('user'));\n    if (userData) {\n      this.user = userData;\n    } else {\n      // Handle case when the user is not logged in (redirect to login page)\n      this.$router.push('/login');\n    }\n  },\n  methods: {\n    updateDetails() {\n      // Simulate API call to save updated user details\n      alert(\"Your details have been updated!\");\n\n      // Save updated details back to localStorage\n      localStorage.setItem('user', JSON.stringify(this.user));\n    },\n    viewOrder(orderId) {\n      alert(`Viewing details for order ID: ${orderId}`);\n      // Optionally, redirect to order details page or open modal\n    },\n    goHome() {\n      this.$router.push('/home'); // Navigate to the homepage\n    },\n    updateSettings() {\n      alert(\"Your settings have been updated!\");\n      // Save updated settings (e.g., API call)\n    }\n  }\n};","map":{"version":3,"names":["data","activeTab","user","name","email","phone","address","orders","id","date","total","settings","newPassword","notifications","mounted","userData","JSON","parse","localStorage","getItem","$router","push","methods","updateDetails","alert","setItem","stringify","viewOrder","orderId","goHome","updateSettings"],"sources":["C:\\laragon\\www\\project_appdev\\Frontend\\src\\views\\profile.vue"],"sourcesContent":["<template>\r\n    <div class=\"profile-page\">\r\n      <div class=\"profile-container\">\r\n        <!-- Sidebar -->\r\n        <aside class=\"profile-sidebar\">\r\n          <ul class=\"profile-menu\">\r\n            <li :class=\"{ active: activeTab === 'details' }\" @click=\"activeTab = 'details'\">\r\n              <span>üë§</span> My Details\r\n            </li>\r\n            <li :class=\"{ active: activeTab === 'orders' }\" @click=\"activeTab = 'orders'\">\r\n              <span>üì¶</span> My Orders\r\n            </li>\r\n            <!-- Back to Homepage Button -->\r\n            <li @click=\"goHome\" class=\"home-button\">\r\n              <span>üè†</span> Home\r\n            </li>\r\n          </ul>\r\n        </aside>\r\n  \r\n        <!-- Main Content -->\r\n        <main class=\"profile-content\">\r\n          <!-- My Details Tab -->\r\n          <section v-if=\"activeTab === 'details'\" class=\"details-tab\">\r\n            <h2>My Details</h2>\r\n            <form @submit.prevent=\"updateDetails\">\r\n              <div class=\"form-group\">\r\n                <label for=\"name\">Full Name</label>\r\n                <input type=\"text\" id=\"name\" v-model=\"user.name\" required />\r\n              </div>\r\n              <div class=\"form-group\">\r\n                <label for=\"email\">Email</label>\r\n                <input type=\"email\" id=\"email\" v-model=\"user.email\" required />\r\n              </div>\r\n              <div class=\"form-group\">\r\n                <label for=\"phone\">Phone Number</label>\r\n                <input type=\"text\" id=\"phone\" v-model=\"user.phone\" />\r\n              </div>\r\n              <div class=\"form-group\">\r\n                <label for=\"address\">Address</label>\r\n                <textarea id=\"address\" v-model=\"user.address\"></textarea>\r\n              </div>\r\n              <button type=\"submit\" class=\"save-button\">Save Changes</button>\r\n            </form>\r\n          </section>\r\n  \r\n          <!-- My Orders Tab -->\r\n          <section v-if=\"activeTab === 'orders'\" class=\"orders-tab\">\r\n            <h2>My Orders</h2>\r\n            <div v-if=\"orders.length > 0\">\r\n              <div v-for=\"order in orders\" :key=\"order.id\" class=\"order-item\">\r\n                <p><strong>Order ID:</strong> {{ order.id }}</p>\r\n                <p><strong>Date:</strong> {{ order.date }}</p>\r\n                <p><strong>Total:</strong> ‚Ç±{{ order.total.toFixed(2) }}</p>\r\n                <button @click=\"viewOrder(order.id)\">View Details</button>\r\n              </div>\r\n            </div>\r\n            <p v-else>No orders found.</p>\r\n          </section>\r\n        </main>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        activeTab: \"details\", // Default active tab\r\n        user: {\r\n          name: \"\",\r\n          email: \"\",\r\n          phone: \"\",\r\n          address: \"\",\r\n        },\r\n        orders: [\r\n          {\r\n            id: \"ORD12345\",\r\n            date: \"2024-11-20\",\r\n            total: 149.99,\r\n          },\r\n          {\r\n            id: \"ORD67890\",\r\n            date: \"2024-10-15\",\r\n            total: 89.99,\r\n          },\r\n        ],\r\n        settings: {\r\n          newPassword: \"\",\r\n          notifications: true,\r\n        },\r\n      };\r\n    },\r\n    mounted() {\r\n      // Fetch user data from localStorage\r\n      const userData = JSON.parse(localStorage.getItem('user'));\r\n      if (userData) {\r\n        this.user = userData;\r\n      } else {\r\n        // Handle case when the user is not logged in (redirect to login page)\r\n        this.$router.push('/login');\r\n      }\r\n    },\r\n    methods: {\r\n      updateDetails() {\r\n        // Simulate API call to save updated user details\r\n        alert(\"Your details have been updated!\");\r\n  \r\n        // Save updated details back to localStorage\r\n        localStorage.setItem('user', JSON.stringify(this.user));\r\n      },\r\n      viewOrder(orderId) {\r\n        alert(`Viewing details for order ID: ${orderId}`);\r\n        // Optionally, redirect to order details page or open modal\r\n      },\r\n      goHome() {\r\n        this.$router.push('/home'); // Navigate to the homepage\r\n      },\r\n      updateSettings() {\r\n        alert(\"Your settings have been updated!\");\r\n        // Save updated settings (e.g., API call)\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* General Page Styling */\r\n  .profile-page {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: flex-start;\r\n    padding: 30px;\r\n    background: linear-gradient(to bottom, #f9f9f9, #e0e0e0);\r\n    font-family: 'Roboto', sans-serif;\r\n  }\r\n  \r\n  .profile-container {\r\n    display: flex;\r\n    width: 90%;\r\n    max-width: 1200px;\r\n    background: #fff;\r\n    border-radius: 15px;\r\n    box-shadow: 0 8px 20px rgba(0, 0, 0, 0.1);\r\n    overflow: hidden;\r\n  }\r\n  \r\n  /* Sidebar */\r\n  .profile-sidebar {\r\n    flex: 1;\r\n    background-color: #f4f4f4;\r\n    padding: 20px;\r\n    border-right: 1px solid #ddd;\r\n  }\r\n  \r\n  .profile-menu {\r\n    list-style: none;\r\n    padding: 0;\r\n    margin: 0;\r\n  }\r\n  \r\n  .profile-menu li {\r\n    padding: 15px 20px;\r\n    margin-bottom: 10px;\r\n    cursor: pointer;\r\n    font-size: 1rem;\r\n    color: #555;\r\n    border-radius: 8px;\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 10px;\r\n    transition: all 0.3s ease;\r\n  }\r\n  \r\n  .profile-menu li:hover,\r\n  .profile-menu li.active {\r\n    background-color: #1e88e5;\r\n    color: #fff;\r\n  }\r\n  \r\n  /* Home Button Styling */\r\n  .home-button {\r\n    background-color: transparent;\r\n  }\r\n  \r\n  .home-button:hover {\r\n    background-color: #1e88e5;\r\n    color: #fff;\r\n  }\r\n  \r\n  /* Main Content *