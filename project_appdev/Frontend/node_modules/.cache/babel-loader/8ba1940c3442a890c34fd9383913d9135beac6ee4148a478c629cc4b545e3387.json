{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport \"core-js/modules/esnext.iterator.reduce.js\";\nimport axios from \"axios\";\nimport _ from 'lodash';\nexport default {\n  data() {\n    return {\n      products: [],\n      filteredProducts: [],\n      searchQuery: '',\n      cartItemCount: 0,\n      loading: true\n    };\n  },\n  created() {\n    this.fetchProducts();\n    this.updateCartItemCount();\n  },\n  methods: {\n    async fetchProducts() {\n      try {\n        const response = await axios.get(\"http://localhost:5555/products\");\n        this.products = response.data.products.map(product => ({\n          ...product,\n          orderQuantity: 1\n        }));\n        this.filteredProducts = this.products; // Initially show all products\n      } catch (error) {\n        console.error(\"Error fetching products:\", error);\n      } finally {\n        this.loading = false;\n      }\n    },\n    // Debounced search method\n    debouncedSearch: _.debounce(function () {\n      this.filteredProducts = this.products.filter(product => product.name.toLowerCase().includes(this.searchQuery.toLowerCase()));\n    }, 300),\n    async addToCart(product) {\n      const cart = JSON.parse(localStorage.getItem(\"cartItems\")) || [];\n      const existingProduct = cart.find(item => item.id === product.id);\n      if (existingProduct) {\n        existingProduct.quantity += product.orderQuantity; // Update quantity if the item exists\n      } else {\n        cart.push({\n          id: product.id,\n          name: product.name,\n          price: product.price,\n          image: `http://localhost:5555${product.image_url}`,\n          quantity: product.orderQuantity\n        });\n      }\n      localStorage.setItem(\"cartItems\", JSON.stringify(cart)); // Save to localStorage\n      this.updateCartItemCount(); // Update the cart count\n    },\n    updateCartItemCount() {\n      const cart = JSON.parse(localStorage.getItem(\"cartItems\")) || [];\n      this.cartItemCount = cart.reduce((total, item) => total + item.quantity, 0);\n    },\n    decrementQuantity(product) {\n      if (product.orderQuantity > 1) {\n        product.orderQuantity--;\n      }\n    },\n    incrementQuantity(product) {\n      if (product.orderQuantity < product.stock) {\n        product.orderQuantity++;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","_","data","products","filteredProducts","searchQuery","cartItemCount","loading","created","fetchProducts","updateCartItemCount","methods","response","get","map","product","orderQuantity","error","console","debouncedSearch","debounce","filter","name","toLowerCase","includes","addToCart","cart","JSON","parse","localStorage","getItem","existingProduct","find","item","id","quantity","push","price","image","image_url","setItem","stringify","reduce","total","decrementQuantity","incrementQuantity","stock"],"sources":["C:\\laragon\\www\\project_appdev\\Frontend\\src\\views\\home.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-page\">\r\n    <!-- Announcement Bar -->\r\n    <div class=\"announcement-bar\">\r\n      üöö Enjoy a 3-Day Return or Refund Guarantee for Every Purchase!\r\n    </div>\r\n\r\n    <!-- Header (Same as home.vue) -->\r\n    <header class=\"header\">\r\n      <div class=\"logo\">Margatheo Store</div>\r\n      <nav class=\"nav\">\r\n        <router-link to=\"/home\" class=\"nav-link\" :class=\"{ active: $route.path === '/home' }\">Home</router-link>\r\n        <router-link to=\"/product\" class=\"nav-link\" :class=\"{ active: $route.path === '/product' }\">Shop</router-link>\r\n      </nav>\r\n      <div class=\"user-controls\">\r\n        <div class=\"user-menu\">\r\n          <img\r\n            src=\"https://libapps-au.s3-ap-southeast-2.amazonaws.com/accounts/12227/images/Circle_Librarian.png\"\r\n            alt=\"User Avatar\"\r\n            class=\"user-avatar\"\r\n          />\r\n          <div class=\"dropdown\">\r\n            <a href=\"/profile\">Profile</a>\r\n            <a href=\"/\">Log Out</a>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </header>\r\n\r\n    <!-- Product Section (Styling only) -->\r\n    <section class=\"products\">\r\n      <h2>Our Products</h2>\r\n      <p class=\"description\">\r\n        Explore our premium collection of school supplies. Quality and style to help you succeed.\r\n      </p>\r\n      <div v-if=\"loading\" class=\"loading\">Loading products...</div>\r\n      <div v-else-if=\"filteredProducts.length > 0\" class=\"product-grid\">\r\n        <div class=\"product-card\" v-for=\"product in filteredProducts\" :key=\"product.id\">\r\n          <div class=\"image-wrapper\">\r\n            <img :src=\"`http://localhost:5555${product.image_url}`\" :alt=\"product.name\" class=\"product-image\" />\r\n            <div class=\"badge\" v-if=\"product.isNew || product.isDiscounted\">\r\n              {{ product.isNew ? \"New\" : product.discount + \"% Off\" }}\r\n            </div>\r\n          </div>\r\n          <div class=\"product-info\">\r\n            <h3 class=\"product-name\">{{ product.name }}</h3>\r\n            <p class=\"product-price\">\r\n              ‚Ç±{{ product.price }} <span class=\"old-price\" v-if=\"product.oldPrice\">‚Ç±{{ product.oldPrice }}</span>\r\n            </p>\r\n            <div class=\"quantity-container\">\r\n              <button @click=\"decrementQuantity(product)\" class=\"quantity-button\" :disabled=\"product.orderQuantity <= 1\">-</button>\r\n              <input v-model.number=\"product.orderQuantity\" type=\"number\" min=\"1\" :max=\"product.stock\" class=\"quantity-input\" />\r\n              <button @click=\"incrementQuantity(product)\" class=\"quantity-button\" :disabled=\"product.orderQuantity >= product.stock\">+</button>\r\n            </div>\r\n            <!-- Add to Cart Button -->\r\n            <button @click=\"addToCart(product)\" class=\"add-to-cart\" :disabled=\"product.orderQuantity <= 0 || product.orderQuantity > product.stock\">Add to Cart</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <p v-else>No products available at the moment.</p>\r\n    </section>\r\n\r\n    <!-- Footer (Same as home.vue) -->\r\n    <footer class=\"footer\">\r\n      <div class=\"footer-contact\">\r\n        <h4>Contact Us</h4>\r\n        <p>Margatheo Store</p>\r\n        <p>5200</p>\r\n        <p>üìû +639150232461</p>\r\n        <p>‚úâÔ∏è margatheostore@gmail.com</p>\r\n      </div>\r\n      <div class=\"footer-bottom\">\r\n        <p>\r\n          &copy; 2024 Margatheo Store. All Rights Reserved. Powered by\r\n          <a href=\"http://localhost:8080/home\" target=\"_blank\">Margatheo&reg;</a>\r\n        </p>\r\n      </div>\r\n    </footer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport _ from 'lodash';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      products: [],\r\n      filteredProducts: [],\r\n      searchQuery: '',\r\n      cartItemCount: 0,\r\n      loading: true,\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchProducts();\r\n    this.updateCartItemCount();\r\n  },\r\n  methods: {\r\n    async fetchProducts() {\r\n      try {\r\n        const response = await axios.get(\"http://localhost:5555/products\");\r\n        this.products = response.data.products.map((product) => ({\r\n          ...product,\r\n          orderQuantity: 1,\r\n        }));\r\n        this.filteredProducts = this.products;  // Initially show all products\r\n      } catch (error) {\r\n        console.error(\"Error fetching products:\", error);\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n\r\n    // Debounced search method\r\n    debouncedSearch: _.debounce(function () {\r\n      this.filteredProducts = this.products.filter((product) =>\r\n        product.name.toLowerCase().includes(this.searchQuery.toLowerCase())\r\n      );\r\n    }, 300),\r\n\r\n    async addToCart(product) {\r\n      const cart = JSON.parse(localStorage.getItem(\"cartItems\")) || [];\r\n      const existingProduct = cart.find((item) => item.id === product.id);\r\n\r\n      if (existingProduct) {\r\n        existingProduct.quantity += product.orderQuantity; // Update quantity if the item exists\r\n      } else {\r\n        cart.push({\r\n          id: product.id,\r\n          name: product.name,\r\n          price: product.price,\r\n          image: `http://localhost:5555${product.image_url}`,\r\n          quantity: product.orderQuantity,\r\n        });\r\n      }\r\n\r\n      localStorage.setItem(\"cartItems\", JSON.stringify(cart)); // Save to localStorage\r\n      this.updateCartItemCount(); // Update the cart count\r\n    },\r\n\r\n    updateCartItemCount() {\r\n      const cart = JSON.parse(localStorage.getItem(\"cartItems\")) || [];\r\n      this.cartItemCount = cart.reduce((total, item) => total + item.quantity, 0);\r\n    },\r\n\r\n    decrementQuantity(product) {\r\n      if (product.orderQuantity > 1) {\r\n        product.orderQuantity--;\r\n      }\r\n    },\r\n\r\n    incrementQuantity(product) {\r\n      if (product.orderQuantity < product.stock) {\r\n        product.orderQuantity++;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Global Styles (Same as home.vue) */\r\nbody {\r\n  margin: 0;\r\n  font-family: \"Poppins\", sans-serif;\r\n  background-color: #f9f9f9;\r\n  color: #333;\r\n}\r\n\r\n/* Announcement Bar (Same as home.vue) */\r\n.announcement-bar {\r\n  background-color: #f4511e;\r\n  color: #fff;\r\n  text-align: center;\r\n  padding: 10px;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n}\r\n\r\n/* Header (Same as home.vue) */\r\n.header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 20px 50px;\r\n  background: #fff;\r\n  box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.logo {\r\n  font-size: 28px;\r\n  font-weight: bold;\r\n  color: #f4511e;\r\n}\r\n\r\n.nav {\r\n  display: flex;\r\n  gap: 20px;\r\n}\r\n\r\n.nav-link {\r\n  text-decoration: none;\r\n  color: #333;\r\n  font-weight: 500;\r\n  padding: 5px;\r\n  transition: color 0.3s ease-in-out;\r\n}\r\n\r\n.nav-link:hover,\r\n.nav-link.active {\r\n  color: #f4511e;\r\n  font-weight: bold;\r\n}\r\n\r\n.user-avatar {\r\n  width: 40px;\r\n  height: 40px;\r\n  border-radius: 50%;\r\n  cursor: pointer;\r\n}\r\n\r\n.user-menu .dropdown {\r\n  display: none;\r\n  position: absolute;\r\n  top: 60px;\r\n  right: 10px;\r\n  background: #fff;\r\n  box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);\r\n  border-radius: 8px;\r\n}\r\n\r\n.user-menu:hover .dropdown {\r\n  display: block;\r\n}\r\n\r\n/* Product Section Styling */\r\n.products {\r\n  padding: 50px 20px;\r\n  background-color: #fff;\r\n  border-radius: 8px;\r\n  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);\r\n  margin: 30px auto;\r\n  max-width: 1200px;\r\n}\r\n\r\n.products h2 {\r\n  font-size: 28px;\r\n  color: #333;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.description {\r\n  margin-bottom: 30px;\r\n  font-size: 16px;\r\n  color: #666;\r\n}\r\n\r\n.product-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\r\n  gap: 20px;\r\n}\r\n\r\n.product-card {\r\n  background: #fff;\r\n  border: 1px solid #eee;\r\n  border-radius: 8px;\r\n  padding: 15px;\r\n  text-align: center;\r\n  box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.image-wrapper {\r\n  position: relative;\r\n}\r\n\r\n.product-image {\r\n  width: 100%;\r\n  height: auto;\r\n  border-radius: 8px;\r\n}\r\n\r\n.badge {\r\n  position: absolute;\r\n  top: 10px;\r\n  left: 10px;\r\n  background: #f4511e;\r\n  color: #fff;\r\n  padding: 5px 10px;\r\n  font-size: 12px;\r\n  border-radius: 20px;\r\n}\r\n\r\n.product-info {\r\n  margin-top: 15px;\r\n}\r\n\r\n.product-name {\r\n  font-size: 18px;\r\n  font-weight: bold;\r\n  color: #333;\r\n}\r\n\r\n.product-price {\r\n  font-size: 16px;\r\n  color: #f4511e;\r\n  margin-top: 10px;\r\n}\r\n\r\n.old-price {\r\n  text-decoration: line-through;\r\n  color: #aaa;\r\n}\r\n\r\n.quantity-container {\r\n  display: flex;\r\n  align-items: center;\r\n  margin-top: 20px;\r\n  gap: 10px;\r\n}\r\n\r\n.quantity-button {\r\n  background-color: #f4511e;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  padding: 10px;\r\n  cursor: pointer;\r\n}\r\n\r\n.quantity-button:disabled {\r\n  background-color: #ccc;\r\n}\r\n\r\n.quantity-input {\r\n  width: 50px;\r\n  text-align: center;\r\n  padding: 5px;\r\n  font-size: 16px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 5px;\r\n}\r\n\r\n.add-to-cart {\r\n  margin-top: 15px;\r\n  background-color: #f4511e;\r\n  color: white;\r\n  padding: 10px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.add-to-cart:disabled {\r\n  background-color: #ccc;\r\n}\r\n\r\n/* Footer (Same as home.vue) */\r\n.footer {\r\n  background-color: #fff;\r\n  padding: 30px 50px;\r\n  margin-top: 30px;\r\n  box-shadow: 0px -4px 10px rgba(0, 0, 0, 0.1);\r\n  text-align: center;\r\n}\r\n\r\n.footer-contact {\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.footer-bottom {\r\n  font-size: 14px;\r\n  color: #777;\r\n}\r\n\r\n.footer-bottom a {\r\n  color: #f4511e;\r\n  text-decoration: none;\r\n}\r\n</style>\r\n"],"mappings":";;;;;;AAkFA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,CAAA,MAAO,QAAQ;AAEtB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,gBAAgB,EAAE,EAAE;MACpBC,WAAW,EAAE,EAAE;MACfC,aAAa,EAAE,CAAC;MAChBC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;IACpB,IAAI,CAACC,mBAAmB,CAAC,CAAC;EAC5B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMF,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,gCAAgC,CAAC;QAClE,IAAI,CAACV,QAAO,GAAIS,QAAQ,CAACV,IAAI,CAACC,QAAQ,CAACW,GAAG,CAAEC,