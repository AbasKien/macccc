{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"SignUpComponent\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      error: null,\n      isVisible: true,\n      // Controls visibility of the sign-up page\n      loading: false // Controls the loading state for the button\n    };\n  },\n  methods: {\n    async handleSignUp() {\n      this.error = null;\n      this.loading = true; // Trigger the loading state\n      try {\n        const response = await axios.post(\"http://localhost:5555/signup\", {\n          email: this.email,\n          password: this.password\n        });\n        if (response.data.success) {\n          this.$router.push(\"/login\");\n        } else {\n          this.error = response.data.message || \"Sign up failed.\";\n        }\n      } catch (err) {\n        this.error = err.response?.data?.message || \"Sign up failed. Please try again.\";\n      } finally {\n        this.loading = false; // Reset loading state\n      }\n    },\n    goToLogin() {\n      this.isVisible = false; // Trigger fade-out\n      setTimeout(() => {\n        this.$router.push(\"/login\"); // Navigate to the login page after fade-out\n      }, 600); // Matches the transition duration\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","error","isVisible","loading","methods","handleSignUp","response","post","success","$router","push","message","err","goToLogin","setTimeout"],"sources":["C:\\laragon\\www\\project_appdev\\Frontend\\src\\views\\loginview.vue"],"sourcesContent":["<template>\r\n  <transition name=\"fade-scale-slide\">\r\n    <section class=\"sign-up-section\" v-if=\"isVisible\">\r\n      <div class=\"container\">\r\n        <!-- Left Section: Image -->\r\n        <div class=\"image-section\"></div>\r\n\r\n        <!-- Right Section: Sign Up Form -->\r\n        <div class=\"login-card shadow rounded-3 p-4\">\r\n          <!-- Logo -->\r\n          <div class=\"text-center mb-4\">\r\n            <img\r\n              src=\"https://media.istockphoto.com/vectors/user-member-vector-icon-for-ui-user-interface-or-profile-face-avatar-vector-id1130884625?k=6&m=1130884625&s=170667a&w=0&h=b4ICEL-2imqnsT-m2tYGxZdxlgD1yKxmoDA-PmPc2-A=\"\r\n              alt=\"Logo\"\r\n              class=\"logo mb-2\"\r\n            />\r\n            <h2 class=\"fs-4 fw-bold text-primary\">Sign Up</h2>\r\n          </div>\r\n\r\n          <!-- Display Error Message -->\r\n          <div v-if=\"error\" class=\"alert alert-danger\" role=\"alert\">\r\n            {{ error }}\r\n          </div>\r\n\r\n          <!-- Sign Up Form -->\r\n          <form @submit.prevent=\"handleSignUp\">\r\n            <!-- Email Input -->\r\n            <div class=\"form-floating mb-3\">\r\n              <input\r\n                type=\"email\"\r\n                class=\"form-control\"\r\n                v-model=\"email\"\r\n                id=\"email\"\r\n                placeholder=\"name@example.com\"\r\n                required\r\n              />\r\n              <label for=\"email\">Email</label>\r\n            </div>\r\n\r\n            <!-- Password Input -->\r\n            <div class=\"form-floating mb-3\">\r\n              <input\r\n                type=\"password\"\r\n                class=\"form-control\"\r\n                v-model=\"password\"\r\n                id=\"password\"\r\n                placeholder=\"Password\"\r\n                required\r\n              />\r\n              <label for=\"password\">Password</label>\r\n            </div>\r\n\r\n            <!-- Sign Up Button -->\r\n            <transition name=\"fade-scale\">\r\n              <div v-if=\"!loading\" class=\"d-grid\">\r\n                <button class=\"btn btn-primary btn-lg\" type=\"submit\">Sign Up</button>\r\n              </div>\r\n              <div v-else class=\"d-grid\">\r\n                <button class=\"btn btn-primary btn-lg\" type=\"button\" disabled>\r\n                  <span class=\"spinner-border spinner-border-sm\" role=\"status\" aria-hidden=\"true\"></span>\r\n                  Loading...\r\n                </button>\r\n              </div>\r\n            </transition>\r\n          </form>\r\n\r\n          <!-- Login and Sign In Links -->\r\n          <div class=\"text-center mt-3\">\r\n            <span class=\"small\">Already have an account? </span>\r\n            <a href=\"javascript:void(0)\" @click=\"goToLogin\" class=\"text-primary small\">Login</a>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </transition>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"SignUpComponent\",\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\",\r\n      error: null,\r\n      isVisible: true, // Controls visibility of the sign-up page\r\n      loading: false, // Controls the loading state for the button\r\n    };\r\n  },\r\n  methods: {\r\n    async handleSignUp() {\r\n      this.error = null;\r\n      this.loading = true; // Trigger the loading state\r\n      try {\r\n        const response = await axios.post(\"http://localhost:5555/signup\", {\r\n          email: this.email,\r\n          password: this.password,\r\n        });\r\n        if (response.data.success) {\r\n          this.$router.push(\"/login\");\r\n        } else {\r\n          this.error = response.data.message || \"Sign up failed.\";\r\n        }\r\n      } catch (err) {\r\n        this.error = err.response?.data?.message || \"Sign up failed. Please try again.\";\r\n      } finally {\r\n        this.loading = false; // Reset loading state\r\n      }\r\n    },\r\n    goToLogin() {\r\n      this.isVisible = false; // Trigger fade-out\r\n      setTimeout(() => {\r\n        this.$router.push(\"/login\"); // Navigate to the login page after fade-out\r\n      }, 600); // Matches the transition duration\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* The same styles as your login page */\r\n\r\n.sign-up-section {\r\n  background: #f7f8fa;\r\n  min-height: 100vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n/* Adjust layout to include an image on the left */\r\n.container {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  align-items: center;\r\n  max-width: 800px;\r\n  background: #ffffff;\r\n  border-radius: 0.75rem;\r\n  overflow: hidden;\r\n  box-shadow: 0 8px 16px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n/* Left Section with Image */\r\n.container .image-section {\r\n  flex: 1;\r\n  background: url('https://i.pinimg.com/736x/04/de/36/04de360b2abb7be10f2f78fcaf800a79.jpg') no-repeat center;\r\n  background-size: cover;\r\n  min-height: 400px;\r\n}\r\n\r\n/* Right Section for Form */\r\n.login-card {\r\n  flex: 1;\r\n  max-width: 400px;\r\n  width: 100%;\r\n  padding: 2rem;\r\n}\r\n\r\n.logo {\r\n  width: 60px;\r\n  height: 60px;\r\n  object-fit: cover;\r\n  border-radius: 50%;\r\n}\r\n\r\n/* Buttons */\r\n.btn-primary {\r\n  background: #ffb547;\r\n  border: none;\r\n  border-radius: 6px;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.btn-primary:hover {\r\n  background: #ff9f33;\r\n  transform: translateY(-1px);\r\n}\r\n\r\n/* Form Labels */\r\n.form-floating > label {\r\n  font-size: 0.85rem;\r\n  color: #6c757d;\r\n}\r\n\r\n.form-floating > input:focus {\r\n  border-color: #ffb547;\r\n  box-shadow: 0 0 0 0.2rem rgba(255, 181, 71, 0.25);\r\n}\r\n\r\n/* Links */\r\n.text-primary {\r\n  color: #ffb547;\r\n  text-decoration: none;\r\n}\r\n\r\n.text-primary:hover {\r\n  color: #ff9f33;\r\n}\r\n\r\n/* Alert Box */\r\n.alert {\r\n  font-size: 0.8rem;\r\n  padding: 0.5rem;\r\n  margin-top: 1rem;\r\n  text-align: center;\r\n  background-color: rgba(255, 0, 0, 0.1);\r\n  border: 1px solid rgba(255, 0, 0, 0.2);\r\n  color: #d9534f;\r\n}\r\n\r\n/* Enhanced Transition Effect */\r\n.fade-scale-slide-enter-active,\r\n.fade-scale-slide-leave-active {\r\n  transition: opacity 0.6s ease, transform 0.6s ease;\r\n}\r\n\r\n.fade-scale-slide-enter,\r\n.fade-scale-slide-leave-to {\r\n  opacity: 0;\r\n  transform: scale(0.95) translateY(30px);\r\n}\r\n\r\n.fade-scale-enter-active,\r\n.fade-scale-leave-active {\r\n  transition: opacity 0.3s ease, transform 0.3s ease;\r\n}\r\n\r\n.fade-scale-enter,\r\n.fade-scale-leave-to {\r\n  opacity: 0;\r\n  transform: scale(0.95);\r\n}\r\n\r\n/* Mobile Responsiveness */\r\n@media (max-width: 576px) {\r\n  .container {\r\n    flex-direction: column;\r\n  }\r\n\r\n  .container .image-section {\r\n    display: none; /* Hide the image on smaller screens */\r\n  }\r\n\r\n  .login-card {\r\n    max-width: 100%;\r\n    padding: 1.5rem;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AA8EA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,IAAI;MACXC,SAAS,EAAE,IAAI;MAAE;MACjBC,OAAO,EAAE,KAAK,CAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI,CAACJ,KAAI,GAAI,IAAI;MACjB,IAAI,CAACE,OAAM,GAAI,IAAI,EAAE;MACrB,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMV,KAAK,CAACW,IAAI,CAAC,8BAA8B,EAAE;UAChER,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QACF,IAAIM,QAAQ,CAACR,IAAI,CAACU,OAAO,EAAE;UACzB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;QAC7B,OAAO;UACL,IAAI,CAACT,KAAI,GAAIK,QAAQ,CAACR,IAAI,CAACa,OAAM,IAAK,iBAAiB;QACzD;MACF,EAAE,OAAOC,GAAG,EAAE;QACZ,IAAI,CAACX,KAAI,GAAIW,GAAG,CAACN,QAAQ,EAAER,IAAI,EAAEa,OAAM,IAAK,mCAAmC;MACjF,UAAU;QACR,IAAI,CAACR,OAAM,GAAI,KAAK,EAAE;MACxB;IACF,CAAC;IACDU,SAASA,CAAA,EAAG;MACV,IAAI,CAACX,SAAQ,GAAI,KAAK,EAAE;MACxBY,UAAU,CAAC,MAAM;QACf,IAAI,CAACL,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC,EAAE;MAC/B,CAAC,EAAE,GAAG,CAAC,EAAE;IACX;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}