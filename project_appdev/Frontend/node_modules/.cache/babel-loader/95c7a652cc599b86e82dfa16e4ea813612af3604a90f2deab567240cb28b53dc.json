    </div>\r\n\r\n      <!-- Cart Summary -->\r\n      <div class=\"summary\">\r\n        <div class=\"total-items\">Total Items: {{ totalItems }}</div>\r\n        <div class=\"total-cost\">Total Cost: \\${{ totalCost.toFixed(2) }}</div>\r\n        <button class=\"btn checkout-button\" @click=\"checkout\">Proceed to Checkout</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      cartItems: [\r\n        { id: 1, name: \"Product 1\", image: \"https://via.placeholder.com/120\", price: 20, quantity: 2 },\r\n        { id: 2, name: \"Product 2\", image: \"https://via.placeholder.com/120\", price: 15, quantity: 1 },\r\n        { id: 3, name: \"Product 3\", image: \"https://via.placeholder.com/120\", price: 30, quantity: 3 }\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    totalItems() {\r\n      return this.cartItems.reduce((sum, item) => sum + item.quantity, 0);\r\n    },\r\n    totalCost() {\r\n      return this.cartItems.reduce((sum, item) => sum + (item.price * item.quantity), 0);\r\n    }\r\n  },\r\n  methods: {\r\n    incrementQuantity(item) {\r\n      item.quantity++;\r\n    },\r\n    decrementQuantity(item) {\r\n      if (item.quantity > 1) {\r\n        item.quantity--;\r\n      }\r\n    },\r\n    removeItem(itemId) {\r\n      this.cartItems = this.cartItems.filter(item => item.id !== itemId);\r\n    },\r\n    checkout() {\r\n      alert(\"Proceeding to checkout!\");\r\n      // Implement checkout logic here (e.g., redirect to checkout page)\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Body Styling */\r\nbody {\r\n  background: #ddd;\r\n  min-height: 100vh;\r\n  display: flex;\r\n  font-family: sans-serif;\r\n  font-size: 0.8rem;\r\n  font-weight: bold;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n/* Title Styling */\r\n.title {\r\n  margin-bottom: 5vh;\r\n}\r\n\r\n/* Card Styling */\r\n.card {\r\n  margin: auto;\r\n  max-width: 950px;\r\n  width: 90%;\r\n  box-shadow: 0 6px 20px 0 rgba(0, 0, 0, 0.19);\r\n  border-radius: 1rem;\r\n  border: transparent;\r\n}\r\n\r\n/* Cart Section Styling */\r\n.cart {\r\n  background-color: #fff;\r\n  padding: 4vh 5vh;\r\n  border-bottom-left-radius: 1rem;\r\n  border-top-left-radius: 1rem;\r\n}\r\n\r\n/* Responsive Cart Padding */\r\n@media(max-width: 767px) {\r\n  .card {\r\n    margin: 3vh auto;\r\n  }\r\n\r\n  .cart {\r\n    padding: 4vh;\r\n    border-bottom-left-radius: unset;\r\n    border-top-right-radius: 1rem;\r\n  }\r\n}\r\n\r\n/* Summary Section Styling */\r\n.summary {\r\n  background-color: #ddd;\r\n  border-top-right-radius: 1rem;\r\n  border-bottom-right-radius: 1rem;\r\n  padding: 4vh;\r\n  color: rgb(65, 65, 65);\r\n}\r\n\r\n/* Responsive Summary Border */\r\n@media(max-width: 767px) {\r\n  .summary {\r\n    border-top-right-radius: unset;\r\n    border-bottom-left-radius: 1rem;\r\n  }\r\n}\r\n\r\n/* Cart Item Styles */\r\n.cart-item {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  background-color: #fff;\r\n  padding: 15px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.cart-item-image {\r\n  width: 120px;\r\n  height: 120px;\r\n  object-fit: cover;\r\n  border-radius: 8px;\r\n  margin-right: 20px;\r\n}\r\n\r\n.cart-item-info {\r\n  flex: 1;\r\n}\r\n\r\n.cart-item-info h2 {\r\n  font-size: 18px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.cart-item-info p {\r\n  font-size: 16px;\r\n  color: #555;\r\n}\r\n\r\n/* Quantity Controls in Cart */\r\n.quantity-container {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 10px;\r\n}\r\n\r\n.quantity-input {\r\n  width: 60px;\r\n  padding: 5px;\r\n  font-size: 14px;\r\n  border-radius: 5px;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n.remove-item {\r\n  background-color: #ff6f61;\r\n  color: white;\r\n  border: none;\r\n  padding: 5px 10px;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.remove-item:hover {\r\n  background-color: #e04e47;\r\n}\r\n\r\n/* Cart Summary Section */\r\n.cart-summary {\r\n  margin-top: 30px;\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.total-items,\r\n.total-cost {\r\n  font-size: 18px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n/* Checkout Button */\r\n.btn {\r\n  background-color: #000;\r\n  border-color: #000;\r\n  color: white;\r\n  width: 100%;\r\n  font-size: 0.7rem;\r\n  margin-top: 4vh;\r\n  padding: 1vh;\r\n  border-radius: 0;\r\n}\r\n\r\n.btn:focus {\r\n  box-shadow: none;\r\n  outline: none;\r\n  color: white;\r\n  transition: none;\r\n}\r\n\r\n.btn:hover {\r\n  color: white;\r\n}\r\n\r\n/* Empty Cart Message */\r\n.empty-cart-message {\r\n  font-size: 18px;\r\n  color: #555;\r\n  text-align: center;\r\n}\r\n\r\na {\r\n  color: black;\r\n}\r\n\r\na:hover {\r\n  color: black;\r\n  text-decoration: none;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAW;;EAKfA,KAAK,EAAC;AAAM;;EACVA,KAAK,EAAC;AAAM;;EAPvBC,GAAA;EASyCD,KAAK,EAAC;;mBAT/C;;EAaiBA,KAAK,EAAC;AAAgB;;EAGpBA,KAAK,EAAC;AAAoB;mBAhB7C;mBAAA;oBAAA;oBAAA;;EAAAC,GAAA;EA0BoBD,KAAK,EAAC;;;EAMfA,KAAK,EAAC;AAAS;;EACbA,KAAK,EAAC;AAAa;;EACnBA,KAAK,EAAC;AAAY;;uBAjC7BE,mBAAA,CAqCM,OArCNC,UAqCM,G,0BApCJC,mBAAA,CAEM;IAFDJ,KAAK,EAAC;EAAkB,IAC3BI,mBAAA,CAA6D,cAAvD,kDAAgD,E,sBAGxDA,mBAAA,CA+BM,OA/BNC,UA+BM,GA9BJD,mBAAA,CAsBM,OAtBNE,UAsBM,G,0BArBJF,mBAAA,CAAyC;IAArCJ,KAAK,EAAC;EAAO,GAAC,oBAAkB,sBACzBO,KAAA,CAAAC,SAAS,CAACC,MAAM,Q,cAA3BP,mBAAA,CAeM,OAfNQ,UAeM,GAdJC,mBAAA,iCAAoC,G,kBACpCT,mBAAA,CAYMU,SAAA,QAvBhBC,WAAA,CAW8BN,KAAA,CAAAC,SAAS,EAAjBM,IAAI;yBAAhBZ,mBAAA,CAYM;MAZ0BD,GAAG,EAAEa,IAAI,CAACC,EAAE;MAAEf,KAAK,EAAC;QAClDI,mBAAA,CAAkE;MAA5DY,GAAG,EAAEF,IAAI,CAACG,KAAK;MAAEC,GAAG,EAAC,YAAY;MAAClB,KAAK,EAAC;4BAZ1DmB,UAAA,GAaYf,mBAAA,CASM,OATNgB,UASM,GARJhB,mBAAA,CAAwB,YAAAiB,gBAAA,CAAjBP,IAAI,CAACQ,IAAI,kBAChBlB,mBAAA,CAAyB,WAAtB,KAAE,GAAAiB,gBAAA,CAAGP,IAAI,CAACS,KAAK,kBAClBnB,mBAAA,CAIM,OAJNoB,UAIM,GAHJpB,mBAAA,C