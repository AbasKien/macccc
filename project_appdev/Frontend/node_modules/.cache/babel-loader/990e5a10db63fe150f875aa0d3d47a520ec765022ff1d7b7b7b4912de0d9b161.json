       <form @submit.prevent=\"submitForm\">\r\n                  <div class=\"row gy-2 overflow-hidden\">\r\n                    <!-- Name Field -->\r\n                    <div class=\"col-12\">\r\n                      <div class=\"form-floating mb-3\">\r\n                        <input v-model=\"name\" type=\"text\" class=\"form-control\" id=\"name\" placeholder=\"Full Name\" required />\r\n                        <label for=\"name\" class=\"form-label\">Full Name</label>\r\n                      </div>\r\n                    </div>\r\n  \r\n                    <!-- Email Field -->\r\n                    <div class=\"col-12\">\r\n                      <div class=\"form-floating mb-3\">\r\n                        <input v-model=\"email\" type=\"email\" class=\"form-control\" id=\"email\" placeholder=\"name@example.com\" required />\r\n                        <label for=\"email\" class=\"form-label\">Email</label>\r\n                      </div>\r\n                    </div>\r\n  \r\n                    <!-- Password Field -->\r\n                    <div class=\"col-12\">\r\n                      <div class=\"form-floating mb-3\">\r\n                        <input v-model=\"password\" type=\"password\" class=\"form-control\" id=\"password\" placeholder=\"Password\" required />\r\n                        <label for=\"password\" class=\"form-label\">Password</label>\r\n                      </div>\r\n                    </div>\r\n  \r\n                    <!-- Submit Button -->\r\n                    <div class=\"col-12\">\r\n                      <div class=\"d-grid my-3\">\r\n                        <button class=\"btn btn-primary btn-lg\" type=\"submit\">Sign Up</button>\r\n                      </div>\r\n                    </div>\r\n  \r\n                    <!-- Already have an account -->\r\n                    <div class=\"col-12\">\r\n                      <p class=\"m-0 text-secondary text-center\">Already have an account? <router-link to=\"/login\" class=\"link-primary text-decoration-none\">Log in</router-link></p>\r\n                    </div>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        name: '',\r\n        email: '',\r\n        password: '',\r\n        error: null\r\n      };\r\n    },\r\n    methods: {\r\n      async submitForm() {\r\n        try {\r\n          const response = await this.$axios.post('/register', {\r\n            name: this.name,\r\n            email: this.email,\r\n            password: this.password\r\n          });\r\n  \r\n          if (response.data.success) {\r\n            // Redirect to login page after successful registration\r\n            this.$router.push('/login');\r\n          } else {\r\n            this.error = response.data.message;\r\n          }\r\n        } catch (err) {\r\n          this.error = 'An error occurred. Please try again.';\r\n        }\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Custom styles can go here, or you can use Bootstrap's built-in utilities */\r\n  </style>\r\n  "],"mappings":";AAmEE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAM,IAAI,CAACC,MAAM,CAACC,IAAI,CAAC,WAAW,EAAE;UACnDR,IAAI,EAAE,IAAI,CAACA,IAAI;UACfC,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;QAEF,IAAII,QAAQ,CAACP,IAAI,CAACU,OAAO,EAAE;UACzB;UACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;QAC7B,OAAO;UACL,IAAI,CAACR,KAAI,GAAIG,QAAQ,CAACP,IAAI,CAACa,OAAO;QACpC;MACF,EAAE,OAAOC,GAAG,EAAE;QACZ,IAAI,CAACV,KAAI,GAAI,sCAAsC;MACrD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}