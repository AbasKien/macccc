{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      name: '',\n      description: '',\n      price: '',\n      image: null\n    };\n  },\n  methods: {\n    handleImageUpload(event) {\n      // Store the selected file in the `image` data property\n      this.image = event.target.files[0];\n    },\n    async addProduct() {\n      // Ensure all fields are filled before proceeding\n      if (!this.name || !this.description || !this.price || !this.image) {\n        alert('All fields are required.');\n        return;\n      }\n      const formData = new FormData();\n      formData.append('name', this.name);\n      formData.append('description', this.description);\n      formData.append('price', this.price);\n      formData.append('image', this.image);\n      try {\n        // Send the form data to the backend API\n        const response = await this.$http.post('/admin/add-product', formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        alert(response.data.message);\n\n        // Redirect to the products page on success\n        if (response.data.success) {\n          this.$router.push('/product'); // Redirect to /product page\n        }\n      } catch (error) {\n        console.error('Error adding product:', error);\n        alert('Failed to add product. Please try again.');\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","name","description","price","image","methods","handleImageUpload","event","target","files","addProduct","alert","formData","FormData","append","response","$http","post","headers","message","success","$router","push","error","console"],"sources":["C:\\laragon\\www\\admin_ecommerce\\vue\\src\\views\\admin.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>Add New Product</h1>\r\n    <form @submit.prevent=\"addProduct\">\r\n      <div>\r\n        <label for=\"name\">Product Name</label>\r\n        <input type=