{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.reduce.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      cartItems: [],\n      // To store cart items fetched from the backend\n      loading: true // To show loading spinner while fetching data\n    };\n  },\n  computed: {\n    // Calculate total items in the cart\n    totalItems() {\n      return this.cartItems.reduce((total, item) => total + item.quantity, 0);\n    },\n    // Calculate total cost of the cart\n    totalCost() {\n      return this.cartItems.reduce((total, item) => total + item.price * item.quantity, 0);\n    }\n  },\n  created() {\n    // Load cart items when the component is mounted\n    this.loadCart();\n  },\n  methods: {\n    // Fetch cart items from the backend\n    async loadCart() {\n      try {\n        this.loading = true; // Start loading\n        const response = await axios.get(\"http://localhost:5555/getCartItems\");\n        if (response.status === 200) {\n          this.cartItems = response.data.items; // Assuming `items` is the array of cart items\n        } else {\n          console.error(\"Failed to fetch cart items:\", response.data.message);\n        }\n      } catch (error) {\n        console.error(\"Error loading cart items:\", error);\n      } finally {\n        this.loading = false; // End loading\n      }\n    },\n    // Update cart item quantity\n    async updateQuantity(item) {\n      if (item.quantity <= 0 || item.quantity > item.stock) {\n        alert(\"Invalid quantity!\");\n        return;\n      }\n      try {\n        const response = await axios.put(`http://localhost:5555/updateCartItem/${item.id}`, {\n          quantity: item.quantity\n        });\n        if (response.status === 200) {\n          console.log(\"Cart updated successfully!\");\n        } else {\n          console.error(\"Failed to update cart:\", response.data.message);\n        }\n      } catch (error) {\n        console.error(\"Error updating cart:\", error);\n      }\n    },\n    // Remove an item from the cart\n    async removeItem(item) {\n      try {\n        const response = await axios.delete(`http://localhost:5555/removeCartItem/${item.id}`);\n        if (response.status === 200) {\n          this.cartItems = this.cartItems.filter(cartItem => cartItem.id !== item.id);\n        } else {\n          console.error(\"Failed to remove item:\", response.data.message);\n        }\n      } catch (error) {\n        console.error(\"Error removing cart item:\", error);\n      }\n    },\n    // Proceed to checkout\n    checkout() {\n      alert(\"Proceeding to checkout...\");\n      this.$router.push(\"/checkout\");\n    }\n  }\n};","map":{"version":3,"names":["axios","data","cartItems","loading","computed","totalItems","reduce","total","item","quantity","totalCost","price","created","loadCart","methods","response","get","status","items","console","error","message","updateQuantity","stock","alert","put","id","log","removeItem","delete","filter","cartItem","checkout","$router","push"],"sources":["C:\\laragon\\www\\project_appdev\\Frontend\\src\\views\\cart.vue"],"sourcesContent":["<template>\r\n  <div class=\"cart-page\">\r\n    <!-- Top Announcement Bar -->\r\n    <div class=\"announcement-bar\">\r\n      Free shipping, 30-day return or refund guarantee.\r\n    </div>\r\n\r\n    <!-- Cart Section -->\r\n    <section class=\"cart-section\">\r\n      <div class=\"cart-content\">\r\n        <h1>Your Cart</h1>\r\n        \r\n        <!-- Cart Items -->\r\n        <div v-if=\"cartItems.length > 0\" class=\"cart-items\">\r\n          <div\r\n            v-for=\"item in cartItems\"\r\n            :key=\"item.id\"\r\n            class=\"cart-item\"\r\n          >\r\n            <img\r\n              :src=\"`http://localhost:5555${item.image_url}`\"\r\n              :alt=\"item.name\"\r\n              class=\"cart-item-image\"\r\n            />\r\n            <div class=\"cart-item-info\">\r\n              <h2>{{ item.name }}</h2>\r\n              <p>Price: ${{ item.price }}</p>\r\n              <div class=\"quantity-container\">\r\n                <input\r\n                  type=\"number\"\r\n                  v-model.number=\"item.quantity\"\r\n                  min=\"1\"\r\n                  :max=\"item.stock\"\r\n                  @change=\"updateQuantity(item)\"\r\n                  class=\"quantity-input\"\r\n                />\r\n                <button @click=\"removeItem(item)\" class=\"remove-item\">Remove</button>\r\n              </div>\r\n              <p class=\"total-price\">Total: ${{ (item.price * item.quantity).toFixed(2) }}</p>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Cart Total Summary -->\r\n          <div class=\"cart-summary\">\r\n            <p class=\"total-items\">Total Items: {{ totalItems }}</p>\r\n            <p class=\"total-cost\">Total Cost: ${{ totalCost.toFixed(2) }}</p>\r\n            <button @click=\"checkout\" class=\"checkout-button\">Proceed to Checkout</button>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Empty Cart Message -->\r\n        <p v-else class=\"empty-cart-message\">Your cart is empty. Start shopping now!</p>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      cartItems: [], // To store cart items fetched from the backend\r\n      loading: true, // To show loading spinner while fetching data\r\n    };\r\n  },\r\n  computed: {\r\n    // Calculate total items in the cart\r\n    totalItems() {\r\n      return this.cartItems.reduce((total, item) => total + item.quantity, 0);\r\n    },\r\n    // Calculate total cost of the cart\r\n    totalCost() {\r\n      return this.cartItems.reduce(\r\n        (total, item) => total + item.price * item.quantity,\r\n        0\r\n      );\r\n    },\r\n  },\r\n  created() {\r\n    // Load cart items when the component is mounted\r\n    this.loadCart();\r\n  },\r\n  methods: {\r\n    // Fetch cart items from the backend\r\n    async loadCart() {\r\n      try {\r\n        this.loading = true; // Start loading\r\n        const response = await axios.get(\"http://localhost:5555/getCartItems\");\r\n\r\n        if (response.status === 200) {\r\n          this.cartItems = response.data.items; // Assuming `items` is the array of cart items\r\n        } else {\r\n          console.error(\"Failed to fetch cart items:\", response.data.message);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error loading cart items:\", error);\r\n      } finally {\r\n        this.loading = false; // End loading\r\n      }\r\n    },\r\n\r\n    // Update cart item quantity\r\n    async updateQuantity(item) {\r\n      if (item.quantity <= 0 || item.quantity > item.stock) {\r\n        alert(\"Invalid quantity!\");\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const response = await axios.put(\r\n          `http://localhost:5555/updateCartItem/${item.id}`,\r\n          { quantity: item.quantity }\r\n        );\r\n\r\n        if (response.status === 200) {\r\n          console.log(\"Cart updated successfully!\");\r\n        } else {\r\n          console.error(\"Failed to update cart:\", response.data.message);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error updating cart:\", error);\r\n      }\r\n    },\r\n\r\n    // Remove an item from the cart\r\n    async removeItem(item) {\r\n      try {\r\n        const response = await axios.delete(\r\n          `http://localhost:5555/removeCartItem/${item.id}`\r\n        );\r\n\r\n        if (response.status === 200) {\r\n          this.cartItems = this.cartItems.filter(\r\n            (cartItem) => cartItem.id !== item.id\r\n          );\r\n        } else {\r\n          console.error(\"Failed to remove item:\", response.data.message);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error removing cart item:\", error);\r\n      }\r\n    },\r\n\r\n    // Proceed to checkout\r\n    checkout() {\r\n      alert(\"Proceeding to checkout...\");\r\n      this.$router.push(\"/checkout\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n/* Cart Page Styles */\r\n.cart-page {\r\n  background-color: #f9f9f9;\r\n  padding: 50px;\r\n}\r\n\r\n.announcement-bar {\r\n  background-color: #070707;\r\n  color: #e33131;\r\n  text-align: center;\r\n  padding: 10px;\r\n  font-size: 14px;\r\n}\r\n\r\n.cart-content {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n\r\nh1 {\r\n  font-size: 36px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.cart-items {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 20px;\r\n}\r\n\r\n.cart-item {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  background-color: #fff;\r\n  padding: 15px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.cart-item-image {\r\n  width: 120px;\r\n  height: 120px;\r\n  object-fit: cover;\r\n  border-radius: 8px;\r\n}\r\n\r\n.cart-item-info {\r\n  flex: 1;\r\n  padding-left: 15px;\r\n}\r\n\r\n.cart-item-info h2 {\r\n  font-size: 18px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.cart-item-info p {\r\n  font-size: 16px;\r\n  color: #555;\r\n}\r\n\r\n.quantity-container {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 10px;\r\n}\r\n\r\n.quantity-input {\r\n  width: 60px;\r\n  padding: 5px;\r\n  font-size: 14px;\r\n  border-radius: 5px;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n.remove-item {\r\n  background-color: #ff6f61;\r\n  color: white;\r\n  border: none;\r\n  padding: 5px 10px;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.remove-item:hover {\r\n  background-color: #e04e47;\r\n}\r\n\r\n.total-price {\r\n  font-size: 16px;\r\n  color: #555;\r\n  margin-top: 10px;\r\n}\r\n\r\n.cart-summary {\r\n  margin-t