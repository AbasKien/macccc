{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"LoginComponent\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      error: null\n    };\n  },\n  methods: {\n    async handleLogin() {\n      this.error = null; // Clear previous errors\n      try {\n        // Send the form data to the server\n        const response = await axios.post('http://localhost:5555/login', {\n          email: this.email,\n          password: this.password\n        });\n\n        // If the response is successful, redirect to the home page\n        if (response.data.verified) {\n          this.$router.push(\"/home\");\n        }\n      } catch (err) {\n        // Check if error message is specifically for unverified email\n        if (err.response && err.response.status === 401 && err.response.data.message === 'Please verify your email before logging in.') {\n          this.error = 'Please verify your email before logging in.';\n        } else {\n          // Generic error message\n          this.error = err.response?.data?.message || \"Login failed. Please try again.\";\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","error","methods","handleLogin","response","post","verified","$router","push","err","status","message"],"sources":["C:\\laragon\\www\\admin_ecommerce\\vue\\src\\views\\loginview.vue"],"sourcesContent":["<template>\r\n  <section class=\"bg-light py-3 py-md-5\">\r\n    <div class=\"container\">\r\n      <div class=\"row justify-content-center\">\r\n        <div class=\"col-12 col-sm-10 col-md-8 col-lg-6 col-xl-5 col-xxl-4\">\r\n          <div class=\"card border border-light-subtle rounded-3 shadow-sm\">\r\n            <div class=\"card-body p-3 p-md-4 p-xl-5\">\r\n              <div class=\"text-center mb-3\">\r\n                <i class=\"bi bi-person-circle\" style=\"font-size: 4rem; color: #007bff;\"></i>\r\n              </div>\r\n              <h2 class=\"fs-6 fw-normal text-center text-secondary mb-4\">Sign in to your account</h2>\r\n\r\n              <!-- Display error message if it exists -->\r\n              <div v-if=\"error\" class=\"alert alert-danger\" role=\"alert\">\r\n                {{ error }}\r\n              </div>\r\n\r\n              <!-- Login form -->\r\n              <form @submit.prevent=\"handleLogin\">\r\n                <div class=\"row gy-2 overflow-hidden\">\r\n                  <div class=\"col-12\">\r\n                    <div class=\"form-floating mb-3\">\r\n                      <input\r\n                        type=\"email\"\r\n                        class=\"form-control\"\r\n                        v-model=\"email\"\r\n                        id=\"email\"\r\n                        placeholder=\"name@example.com\"\r\n                        required\r\n                      />\r\n                      <label for=\"email\" class=\"form-label\">Email</label>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-12\">\r\n                    <div class=\"form-floating mb-3\">\r\n                      <input\r\n                        type=\"password\"\r\n                        class=\"form-control\"\r\n                        v-model=\"password\"\r\n                        id=\"password\"\r\n                        placeholder=\"Password\"\r\n                        required\r\n                      />\r\n                      <label for=\"password\" class=\"form-label\">Password</label>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-12\">\r\n                    <div class=\"d-grid my-3\">\r\n                      <button class=\"btn btn-primary btn-lg\" type=\"submit\">Log in</button>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-12\">\r\n                    <p class=\"m-0 text-secondary text-center\">\r\n                      Don't have an account?\r\n                      <router-link to=\"/register\" class=\"link-primary text-decoration-none\">Sign up</router-link>\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"LoginComponent\",\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\",\r\n      error: null,\r\n    };\r\n  },\r\n  methods: {\r\n    async handleLogin() {\r\n      this.error = null; // Clear previous errors\r\n      try {\r\n        // Send the form data to the server\r\n        const response = await axios.post('http://localhost:5555/login', {\r\n          email: this.email,\r\n          password: this.password\r\n        });\r\n\r\n        // If the response is successful, redirect to the home page\r\n        if (response.data.verified) {\r\n          this.$router.push(\"/home\");\r\n        }\r\n      } catch (err) {\r\n        // Check if error message is specifically for unverified email\r\n        if (err.response && err.response.status === 401 && err.response.data.message === 'Please verify your email before logging in.') {\r\n          this.error = 'Please verify your email before logging in.';\r\n        } else {\r\n          // Generic error message\r\n          this.error = err.response?.data?.message || \"Login failed. Please try again.\";\r\n        }\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Your existing styles here */\r\n</style>\r\n"],"mappings":";AAoEA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI,CAACF,KAAI,GAAI,IAAI,EAAE;MACnB,IAAI;QACF;QACA,MAAMG,QAAO,G