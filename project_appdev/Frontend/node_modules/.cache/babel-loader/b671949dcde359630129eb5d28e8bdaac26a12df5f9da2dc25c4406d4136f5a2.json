{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nexport default {\n  data() {\n    return {\n      stats: [{\n        label: \"Product Varieties\",\n        value: \"1k+\"\n      }, {\n        label: \"Happy Students\",\n        value: \"10k+\"\n      }, {\n        label: \"Store Locations\",\n        value: \"5+\"\n      }],\n      cards: [{\n        title: \"Wide Variety of Notebooks\",\n        description: \"Find notebooks in every size and style for your needs.\",\n        bgColor: \"primary\",\n        icon: \"notebooks\"\n      }, {\n        title: \"Stationery Essentials\",\n        description: \"Pens, pencils, and all the writing tools you need.\",\n        bgColor: \"secondary\",\n        icon: \"stationery\"\n      }, {\n        title: \"Durable Backpacks\",\n        description: \"Carry everything with our top-quality backpacks.\",\n        bgColor: \"danger\",\n        icon: \"backpacks\"\n      }],\n      products: [],\n      // Populate with data from your API or props\n      cartItems: []\n    };\n  },\n  methods: {\n    addToCart(product) {\n      const existingItem = this.cartItems.find(item => item.id === product.id);\n      if (existingItem) {\n        existingItem.quantity += product.quantity;\n      } else {\n        this.cartItems.push({\n          ...product,\n          quantity: product.quantity\n        });\n      }\n      this.updateCartUI();\n    },\n    updateCartUI() {\n      const cartCount = this.cartItems.length;\n      this.$refs.cartCount.textContent = cartCount;\n      // Additional UI updates as needed\n    }\n  }\n};","map":{"version":3,"names":["data","stats","label","value","cards","title","description","bgColor","icon","products","cartItems","methods","addToCart","product","existingItem","find","item","id","quantity","push","updateCartUI","cartCount","length","$refs","textContent"],"sources":["C:\\laragon\\www\\admin_ecommerce\\vue\\src\\views\\home.vue"],"sourcesContent":["<template>\r\n    <section style=\"background-image: url('@/assets/images/aa.png'); background-repeat: no-repeat; background-size: cover;\">\r\n      <div class=\"container-lg\">\r\n        <div class=\"row\">\r\n          <div class=\"col-lg-6 pt-5 mt-5\">\r\n            <h2 class=\"display-1 ls-1\">\r\n              <span class=\"fw-bold text-primary\">School</span> Supplies at your <span class=\"fw-bold\">Doorsteps</span>\r\n            </h2>\r\n            <p class=\"fs-4\">Everything you need for a successful school year.</p>\r\n            <div class=\"d-flex gap-3\">\r\n              <router-link to=\"/product\" class=\"btn btn-primary text-uppercase fs-6 rounded-pill px-4 py-3 mt-3\">Start Shopping</router-link>\r\n              <router-link to=\"/join\" class=\"btn btn-dark text-uppercase fs-6 rounded-pill px-4 py-3 mt-3\">Join Now</router-link>\r\n            </div>\r\n            <div class=\"row my-5\">\r\n              <div class=\"col\" v-for=\"stat in stats\" :key=\"stat.label\">\r\n                <div class=\"row text-dark\">\r\n                  <div class=\"col-auto\"><p class=\"fs-1 fw-bold lh-sm mb-0\">{{ stat.value }}</p></div>\r\n                  <div class=\"col\"><p class=\"text-uppercase lh-sm mb-0\">{{ stat.label }}</p></div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n  \r\n        <div class=\"row row-cols-1 row-cols-sm-3 row-cols-lg-3 g-0 justify-content-center\">\r\n          <div v-for=\"card in cards\" :key=\"card.title\" class=\"col\">\r\n            <div :class=\"`card border-0 rounded-0 p-4 text-light bg-${card.bgColor}`\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-3 text-center\">\r\n                  <svg width=\"60\" height=\"60\"><use :xlink:href=\"`#${card.icon}`\"></use></svg>\r\n                </div>\r\n                <div class=\"col-md-9\">\r\n                  <div class=\"card-body p-0\">\r\n                    <h5 class=\"text-light\">{{ card.title }}</h5>\r\n                    <p class=\"card-text\">{{ card.description }}</p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  \r\n    <section class=\"pb-5\">\r\n      <div class=\"container-lg\">\r\n        <div class=\"row\">\r\n          <div class=\"col-md-12\">\r\n            <div class=\"section-header d-flex flex-wrap justify-content-between my-4\">\r\n              <h2 class=\"section-title\">Best Selling Products</h2>\r\n              <router-link to=\"/product\" class=\"btn btn-primary rounded-1\">View All</router-link>\r\n            </div>\r\n          </div>\r\n        </div>\r\n  \r\n        <div class=\"row\">\r\n          <div class=\"col-md-12\">\r\n            <div class=\"product-grid row row-cols-1 row-cols-sm-2 row-cols-md-3 row-cols-lg-3 row-cols-xl-4 row-cols-xxl-5\">\r\n              <div v-for=\"product in products.slice(0, 5)\" :key=\"product.id\" class=\"col\">\r\n                <div class=\"product-item\" @mouseover=\"hover = true\" @mouseleave=\"hover = false\">\r\n                  <figure>\r\n                    <router-link :to=\"{ name: 'Product', params: { id: product.id } }\" :title=\"product.name\">\r\n                      <img :src=\"product.image\" :alt=\"product.name\" class=\"tab-image\" style=\"width: 100%; height: 200px; object-fit: cover;\">\r\n                    </router-link>\r\n                  </figure>\r\n                  <div class=\"d-flex flex-column text-center\">\r\n                    <h3 class=\"fs-6 fw-normal\">{{ product.name }}</h3>\r\n                    <div>\r\n                      <span class=\"rating\">\r\n                        <svg width=\"18\" height=\"18\" class=\"text-warning\" v-for=\"i in 4\" :key=\"i\"><use xlink:href=\"#star-full\"></use></svg>\r\n                        <svg width=\"18\" height=\"18\" class=\"text-warning\"><use xlink:href=\"#star-half\"></use></svg>\r\n                      </span>\r\n                      <span>(4)</span>\r\n                    </div>\r\n                    <div class=\"d-flex justify-content-center align-items-center gap-2\">\r\n                      <del>${{ product.price + 6 }}</del>\r\n                      <span class=\"text-dark fw-semibold\">${{ product.price }}</span>\r\n                      <span class=\"badge border border-dark-subtle rounded-0 fw-normal px-1 fs-7 lh-1 text-body-tertiary\">10% OFF</span>\r\n                    </div>\r\n                    <div class=\"button-area p-3 pt-0\">\r\n                      <div class=\"row g-1 mt-2\">\r\n                        <div class=\"col-3\">\r\n                          <input type=\"number\" name=\"quantity\" class=\"form-control border-dark-subtle input-number quantity\" v-model.number=\"product.quantity\" min=\"1\" />\r\n                        </div>\r\n                        <div class=\"col-9\">\r\n                          <button type=\"button\" class=\"btn btn-primary rounded-1 p-2 fs-7 btn-cart\" @click=\"addToCart(product)\">\r\n                            <svg width=\"18\" height=\"18\"><use xlink:href=\"#cart\"></use></svg> Add to Cart\r\n                          </button>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <!-- /product-grid -->\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        stats: [\r\n          { label: \"Product Varieties\", value: \"1k+\" },\r\n          { label: \"Happy Students\", value: \"10k+\" },\r\n          { label: \"Store Locations\", value: \"5+\" },\r\n        ],\r\n        cards: [\r\n          { title: \"Wide Variety of Notebooks\", description: \"Find notebooks in every size and style for your needs.\", bgColor: \"primary\", icon: \"notebooks\" },\r\n          { title: \"Stationery Essentials\", description: \"Pens, pencils, and all the writing tools you need.\", bgColor: \"secondary\", icon: \"stationery\" },\r\n          { title: \"Durable Backpacks\", description: \"Carry everything with our top-quality backpacks.\", bgColor: \"danger\", icon: \"backpacks\" },\r\n        ],\r\n        products: [], // Populate with data from your API or props\r\n        cartItems: [],\r\n      };\r\n    },\r\n    methods: {\r\n      addToCart(product) {\r\n        const existingItem = this.cartItems.find(item => item.id === product.id);\r\n        if (existingItem) {\r\n          existingItem.quantity += product.quantity;\r\n        } else {\r\n          this.cartItems.push({ ...product, quantity: product.quantity });\r\n        }\r\n        this.updateCartUI();\r\n      },\r\n      updateCartUI() {\r\n        const cartCount = this.cartItems.length;\r\n        this.$refs.cartCount.textContent = cartCount;\r\n        // Additional UI updates as needed\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .product-item {\r\n    transition: transform 0.2s ease, box-shadow 0.2s ease;\r\n    border: 1px solid #e1e1e1;\r\n    border-radius: 8px;\r\n  }\r\n  .product-item:hover {\r\n    transform: translateY(-5px);\r\n    box-shadow: 0 4px 15px rgba(0, 0, 0, 0.2);\r\n  }\r\n  .product-item figure:hover img {\r\n    opacity: 0.8;\r\n    transition: opacity 0.2s ease;\r\n  }\r\n  .animate-cart {\r\n    transform: scale(1.1);\r\n    transition: transform 0.3s ease-in-out;\r\n  }\r\n  .scale-up {\r\n    transform: scale(1.2);\r\n    transition: transform 0.3s ease-in-out;\r\n  }\r\n  .cart-icon {\r\n    position: relative;\r\n    display: inline-block;\r\n  }\r\n  #cartItemCount {\r\n    position: absolute;\r\n    top: -8px;\r\n    right: -8px;\r\n    background-color: red;\r\n    color: white;\r\n    padding: 2px 6px;\r\n    font-size: 12px;\r\n    border-radius: 50%;\r\n  }\r\n  </style>\r\n  "],"mappings":";;;AAuGE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QAAEC,KAAK,EAAE,mBAAmB;QAAEC,KAAK,EAAE;MAAM,CAAC,EAC5C;QAAED,KAAK,EAAE,gBAAgB;QAAEC,KAAK,EAAE;MAAO,CAAC,EAC1C;QAAED,KAAK,EAAE,iBAAiB;QAAEC,KAAK,EAAE;MAAK,CAAC,CAC1C;MACDC,KAAK,EAAE,CACL;QAAEC,KAAK,EAAE,2BAA2B;QAAEC,WAAW,EAAE,wDAAwD;QAAEC,OAAO,EAAE,SAAS;QAAEC,IAAI,EAAE;MAAY,CAAC,EACpJ;QAAEH,KAAK,EAAE,uBAAuB;QAAEC,WAAW,EAAE,oDAAoD;QAAEC,OAAO,EAAE,WAAW;QAAEC,IAAI,EAAE;MAAa,CAAC,EAC/I;QAAEH,KAAK,EAAE,mBAAmB;QAAEC,WAAW,EAAE,kDAAkD;QAAEC,OAAO,EAAE,QAAQ;QAAEC,IAAI,EAAE;MAAY,CAAC,CACtI;MACDC,QAAQ,EAAE,EAAE;MAAE;MACdC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAACC,OAAO,EAAE;MACjB,MAAMC,YAAW,GAAI,IAAI,CAACJ,SAAS,CAACK,IAAI,CAACC,IAAG,IAAKA,IAAI,CAACC,EAAC,KAAMJ,OAAO,CAACI,EAAE,CAAC;MACxE,IAAIH,YAAY,EAAE;QAChBA,YAAY,CAACI,QAAO,IAAKL,OAAO,CAACK,QAAQ;MAC3C,OAAO;QACL,IAAI,CAACR,SAAS,CAACS,IAAI,CAAC;UAAE,GAAGN,OAAO;UAAEK,QAAQ,EAAEL,OAAO,CAACK;QAAS,CAAC,CAAC;MACjE;MACA,IAAI,CAACE,YAAY,CAAC,CAAC;IACrB,CAAC;IACDA,YAAYA,CAAA,EAAG;MACb,MAAMC,SAAQ,GAAI,IAAI,CAACX,SAAS,CAACY,MAAM;MACvC,IAAI,CAACC,KAAK,CAACF,SAAS,CAACG,WAAU,GAAIH,SAAS;MAC5C;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}