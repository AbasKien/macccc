{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.reduce.js\";\nexport default {\n  data() {\n    return {\n      cartItems: [{\n        id: 1,\n        name: \"Product 1\",\n        image: \"https://via.placeholder.com/120\",\n        price: 20,\n        quantity: 2\n      }, {\n        id: 2,\n        name: \"Product 2\",\n        image: \"https://via.placeholder.com/120\",\n        price: 15,\n        quantity: 1\n      }, {\n        id: 3,\n        name: \"Product 3\",\n        image: \"https://via.placeholder.com/120\",\n        price: 30,\n        quantity: 3\n      }]\n    };\n  },\n  computed: {\n    totalItems() {\n      return this.cartItems.reduce((sum, item) => sum + item.quantity, 0);\n    },\n    totalCost() {\n      return this.cartItems.reduce((sum, item) => sum + item.price * item.quantity, 0);\n    }\n  },\n  methods: {\n    incrementQuantity(item) {\n      item.quantity++;\n    },\n    decrementQuantity(item) {\n      if (item.quantity > 1) {\n        item.quantity--;\n      }\n    },\n    removeItem(itemId) {\n      this.cartItems = this.cartItems.filter(item => item.id !== itemId);\n    },\n    checkout() {\n      alert(\"Proceeding to checkout!\");\n      // Implement checkout logic here (e.g., redirect to checkout page)\n    }\n  }\n};","map":{"version":3,"names":["data","cartItems","id","name","image","price","quantity","computed","totalItems","reduce","sum","item","totalCost","methods","incrementQuantity","decrementQuantity","removeItem","itemId","filter","checkout","alert"],"sources":["C:\\laragon\\www\\project_appdev\\Frontend\\src\\views\\cart.vue"],"sourcesContent":["<template>\r\n  <div class=\"cart-page\">\r\n    <div class=\"announcement-bar\">\r\n      <span>Special Offer: Free Shipping on Orders Over $50!</span>\r\n    </div>\r\n\r\n    <div class=\"cart-content\">\r\n      <h1>Your Shopping Cart</h1>\r\n      <div v-if=\"cartItems.length > 0\" class=\"cart-items\">\r\n        <!-- Loop through each cart item -->\r\n        <div v-for=\"item in cartItems\" :key=\"item.id\" class=\"cart-item\">\r\n          <img :src=\"item.image\" alt=\"Item image\" class=\"cart-item-image\" />\r\n          <div class=\"cart-item-info\">\r\n            <h2>{{ item.name }}</h2>\r\n            <p>\\${{ item.price }}</p>\r\n            <div class=\"quantity-container\">\r\n              <button @click=\"decrementQuantity(item)\" :disabled=\"item.quantity <= 1\">-</button>\r\n              <input type=\"number\" v-model=\"item.quantity\" :min=\"1\" class=\"quantity-input\" />\r\n              <button @click=\"incrementQuantity(item)\">+</button>\r\n            </div>\r\n            <button @click=\"removeItem(item.id)\" class=\"remove-item\">Remove</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div v-else class=\"empty-cart-message\">\r\n        <p>Your cart is empty. Start shopping now!</p>\r\n      </div>\r\n\r\n      <!-- Cart Summary -->\r\n      <div class=\"cart-summary\">\r\n        <div class=\"total-items\">Total Items: {{ totalItems }}</div>\r\n        <div class=\"total-cost\">Total Cost: \\${{ totalCost.toFixed(2) }}</div>\r\n        <button class=\"checkout-button\" @click=\"checkout\">Proceed to Checkout</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      cartItems: [\r\n        { id: 1, name: \"Product 1\", image: \"https://via.placeholder.com/120\", price: 20, quantity: 2 },\r\n        { id: 2, name: \"Product 2\", image: \"https://via.placeholder.com/120\", price: 15, quantity: 1 },\r\n        { id: 3, name: \"Product 3\", image: \"https://via.placeholder.com/120\", price: 30, quantity: 3 }\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    totalItems() {\r\n      return this.cartItems.reduce((sum, item) => sum + item.quantity, 0);\r\n    },\r\n    totalCost() {\r\n      return this.cartItems.reduce((sum, item) => sum + (item.price * item.quantity), 0);\r\n    }\r\n  },\r\n  methods: {\r\n    incrementQuantity(item) {\r\n      item.quantity++;\r\n    },\r\n    decrementQuantity(item) {\r\n      if (item.quantity > 1) {\r\n        item.quantity--;\r\n      }\r\n    },\r\n    removeItem(itemId) {\r\n      this.cartItems = this.cartItems.filter(item => item.id !== itemId);\r\n    },\r\n    checkout() {\r\n      alert(\"Proceeding to checkout!\");\r\n      // Implement checkout logic here (e.g., redirect to checkout page)\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Global Styles for Cart Page */\r\n.cart-page {\r\n  background-color: #f9f9f9;\r\n  padding: 50px;\r\n}\r\n\r\n.announcement-bar {\r\n  background-color: #070707;\r\n  color: #e33131;\r\n  text-align: center;\r\n  padding: 10px;\r\n  font-size: 14px;\r\n}\r\n\r\n.cart-content {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n\r\nh1 {\r\n  font-size: 36px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n/* Cart Item Styles */\r\n.cart-items {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 20px;\r\n}\r\n\r\n.cart-item {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  background-color: #fff;\r\n  padding: 15px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.cart-item-image {\r\n  width: 120px;\r\n  height: 120px;\r\n  object-fit: cover;\r\n  border-radius: 8px;\r\n  margin-right: 20px;\r\n}\r\n\r\n.cart-item-info {\r\n  flex: 1;\r\n}\r\n\r\n.cart-item-info h2 {\r\n  font-size: 18px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.cart-item-info p {\r\n  font-size: 16px;\r\n  color: #555;\r\n}\r\n\r\n/* Quantity Controls in Cart */\r\n.quantity-container {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 10px;\r\n}\r\n\r\n.quantity-input {\r\n  width: 60px;\r\n  padding: 5px;\r\n  font-size: 14px;\r\n  border-radius: 5px;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n.remove-item {\r\n  background-color: #ff6f61;\r\n  color: white;\r\n  border: none;\r\n  padding: 5px 10px;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.remove-item:hover {\r\n  background-color: #e04e47;\r\n}\r\n\r\n/* Cart Summary Section */\r\n.cart-summary {\r\n  margin-top: 30px;\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.total-price {\r\n  font-size: 16px;\r\n  color: #555;\r\n  margin-top: 10px;\r\n}\r\n\r\n.total-items,\r\n.total-cost {\r\n  font-size: 18px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n/* Checkout Button */\r\n.checkout-button {\r\n  background-color: #ebcf35;\r\n  color: white;\r\n  border: none;\r\n  padding: 10px 20px;\r\n  cursor: pointer;\r\n  font-size: 16px;\r\n  border-radius: 5px;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.checkout-button:hover {\r\n  background-color: #63d031;\r\n}\r\n\r\n/* Empty Cart Message */\r\n.empty-cart-message {\r\n  font-size: 18px;\r\n  color: #555;\r\n  text-align: center;\r\n}\r\n\r\n/* Responsive Design */\r\n@media(max-width: 767px) {\r\n  .cart-item {\r\n    flex-direction: column;\r\n    align-items: flex-start;\r\n  }\r\n\r\n  .cart-item-info {\r\n    margin-left: 0;\r\n  }\r\n\r\n  .cart-summary {\r\n    margin-top: 20px;\r\n    padding: 3vh 4vh;\r\n  }\r\n\r\n  .cart-item-image {\r\n    width: 80px;\r\n    height: 80px;\r\n  }\r\n\r\n  .quantity-input {\r\n    width: 50px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;;AAwCA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,CACT;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE,iCAAiC;QAAEC,KAAK,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAE,CAAC,EAC9F;QAAEJ,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE,iCAAiC;QAAEC,KAAK,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAE,CAAC,EAC9F;QAAEJ,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE,iCAAiC;QAAEC,KAAK,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAE;IAEjG,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAA