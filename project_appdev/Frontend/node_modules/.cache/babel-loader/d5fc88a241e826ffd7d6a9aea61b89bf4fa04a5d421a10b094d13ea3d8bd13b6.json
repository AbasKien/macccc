\n                  {{ error }}\r\n                </div>\r\n  \r\n                <!-- Login form -->\r\n                <form @submit.prevent=\"handleLogin\">\r\n                  <div class=\"row gy-2 overflow-hidden\">\r\n                    <div class=\"col-12\">\r\n                      <div class=\"form-floating mb-3\">\r\n                        <input\r\n                          type=\"email\"\r\n                          class=\"form-control\"\r\n                          v-model=\"email\"\r\n                          id=\"email\"\r\n                          placeholder=\"name@example.com\"\r\n                          required\r\n                        />\r\n                        <label for=\"email\" class=\"form-label\">Email</label>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"col-12\">\r\n                      <div class=\"form-floating mb-3\">\r\n                        <input\r\n                          type=\"password\"\r\n                          class=\"form-control\"\r\n                          v-model=\"password\"\r\n                          id=\"password\"\r\n                          placeholder=\"Password\"\r\n                          required\r\n                        />\r\n                        <label for=\"password\" class=\"form-label\">Password</label>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"col-12\">\r\n                      <div class=\"d-flex gap-2 justify-content-between\">\r\n                        <div class=\"form-check\">\r\n                          <input\r\n                            class=\"form-check-input\"\r\n                            type=\"checkbox\"\r\n                            v-model=\"rememberMe\"\r\n                            id=\"rememberMe\"\r\n                          />\r\n                          <label class=\"form-check-label text-secondary\" for=\"rememberMe\">Keep me logged in