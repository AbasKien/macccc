{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      name: '',\n      description: '',\n      price: '',\n      image: null\n    };\n  },\n  methods: {\n    onFileChange(event) {\n      this.image = event.target.files[0];\n    },\n    async handleAddProduct() {\n      try {\n        const formData = new FormData();\n        formData.append('name', this.name);\n        formData.append('description', this.description);\n        formData.append('price', this.price);\n        if (this.image) {\n          formData.append('image', this.image);\n        }\n        const response = await axios.post('http://localhost:5555/admin/add-product', formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        if (response.data.success) {\n          alert('Product added successfully');\n          this.$router.push('/');\n        } else {\n          alert('Failed to add product');\n        }\n      } catch (error) {\n        console.error('Error:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","name","description","price","image","methods","onFileChange","event","target","files","handleAddProduct","formData","FormData","append","response","post","headers","success","alert","$router","push","error","console"],"sources":["C:\\laragon\\www\\admin_ecommerce\\vue\\src\\views\\admin.vue"],"sourcesContent":["<template>\r\n    <div class=\"admin-container\">\r\n      <h2>Add New Product</h2>\r\n      <form @submit.prevent=\"handleAddProduct\">\r\n        <div class=\"mb-3\">\r\n          <label for=\"name\" class=\"form-label\">Product Name</label>\r\n          <input type=\"text\" v-model=\"name\" class=\"form-control\" required />\r\n        </div>\r\n        <div class=\"mb-3\">\r\n          <label for=\"description\" class=\"form-label\">Description</label>\r\n          <textarea v-model=\"description\" class=\"form-control\" rows=\"3\"></textarea>\r\n        </div>\r\n        <div class=\"mb-3\">\r\n          <label for=\"price\" class=\"form-label\">Price</label>\r\n          <input type=\"number\" v-model=\"price\" class=\"form-control\" required />\r\n        </div>\r\n        <div class=\"mb-3\">\r\n          <label for=\"image\" class=\"form-label\">Image</label>\r\n          <input type=\"file\" @change=\"onFileChange\" class=\"form-control\" />\r\n        </div>\r\n        <button type=\"submit\" class=\"btn btn-primary\">Add Product</button>\r\n      </form>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        name: '',\r\n        description: '',\r\n        price: '',\r\n        image: null\r\n      };\r\n    },\r\n    methods: {\r\n      onFileChange(event) {\r\n        this.image = event.target.files[0];\r\n      },\r\n      async handleAddProduct() {\r\n        try {\r\n          const formData = new FormData();\r\n          formData.append('name', this.name);\r\n          formData.append('description', this.description);\r\n          formData.append('price', this.price);\r\n          if (this.image) {\r\n            formData.append('image', this.image);\r\n          }\r\n  \r\n          const response = await axios.post('http://localhost:5555/admin/add-product', formData, {\r\n            headers: { 'Content-Type': 'multipart/form-data' }\r\n          });\r\n  \r\n          if (response.data.success) {\r\n            alert('Product added successfully');\r\n            this.$router.push('/');\r\n          } else {\r\n            alert('Failed to add product');\r\n          }\r\n        } catch (error) {\r\n          console.error('Error:', error);\r\n        }\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Add your styling here */\r\n  </style>\r\n  "],"mappings":";AA0BE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL