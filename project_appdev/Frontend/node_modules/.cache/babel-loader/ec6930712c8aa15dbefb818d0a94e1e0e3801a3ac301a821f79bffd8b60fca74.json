              </div>\r\n                                            </div>\r\n\r\n                                            <!-- Password Field -->\r\n                                            <div class=\"col-12\">\r\n                                                <div class=\"form-floating mb-3\">\r\n                                                    <input\r\n                                                        type=\"password\"\r\n                                                        class=\"form-control\"\r\n                                                        v-model=\"password\"\r\n                                                        id=\"password\"\r\n                                                        placeholder=\"Password\"\r\n                                                        required\r\n                                                    />\r\n                                                    <label for=\"password\" class=\"form-label\">Password</label>\r\n                                                </div>\r\n                                            </div>\r\n\r\n                                            <!-- Submit Button -->\r\n                                            <div class=\"col-12\">\r\n                                                <div class=\"d-grid my-3\">\r\n                                                    <button class=\"btn btn-primary btn-lg\" type=\"submit\">Sign Up</button>\r\n                                                </div>\r\n                                            </div>\r\n\r\n                                            <!-- Already have an account -->\r\n                                            <div class=\"col-12\">\r\n                                                <p class=\"m-0 text-secondary text-center\">\r\n                                                    Already have an account? \r\n                                                    <span class=\"link-primary text-decoration-none\" @click=\"toggleForm\">Log in</span>\r\n                                                </p>\r\n                                            </div>\r\n                                        </div>\r\n                                    </form>\r\n\r\n                                    <!-- Login Form -->\r\n                                    <form v-if=\"isLogin\" @submit.prevent=\"handleLogin\">\r\n                                        <div class=\"row gy-2 overflow-hidden\">\r\n                                            <!-- Email Field -->\r\n                                            <div class=\"col-12\">\r\n                                                <div class=\"form-floating mb-3\">\r\n                                                    <input\r\n                                                        type=\"email\"\r\n                                                        class=\"form-control\"\r\n                                                        v-model=\"loginEmail\"\r\n                                                        id=\"login-email\"\r\n                                                        placeholder=\"Email\"\r\n                                                        required\r\n                                                    />\r\n                                                    <label for=\"login-email\" class=\"form-label\">Email</label>\r\n                                                </div>\r\n                                            </div>\r\n\r\n                                            <!-- Password Field -->\r\n                                            <div class=\"col-12\">\r\n                                                <div class=\"form-floating mb-3\">\r\n                                                    <input\r\n                                                        type=\"password\"\r\n                                                        class=\"form-control\"\r\n                                                        v-model=\"loginPassword\"\r\n                                                        id=\"login-password\"\r\n                                                        placeholder=\"Password\"\r\n                                                        required\r\n                                                    />\r\n                                                    <label for=\"login-password\" class=\"form-label\">Password</label>\r\n                                                </div>\r\n                                            </div>\r\n\r\n                                            <!-- Submit Button -->\r\n                                            <div class=\"col-12\">\r\n                                                <div class=\"d-grid my-3\">\r\n                                                    <button class=\"btn btn-primary btn-lg\" type=\"submit\">Log In</button>\r\n                                                </div>\r\n                                            </div>\r\n\r\n                                            <!-- Don't have an account -->\r\n                                            <div class=\"col-12\">\r\n                                                <p class=\"m-0 text-secondary text-center\">\r\n                                                    Don't have an account? \r\n                                                    <span class=\"link-primary text-decoration-none\" @click=\"toggleForm\">Sign up</span>\r\n                                                </p>\r\n                                            </div>\r\n                                        </div>\r\n                                    </form>\r\n                                </div>\r\n                            </transition>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    name: \"RegisterComponent\",\r\n    data() {\r\n        return {\r\n            isLogin: false,  // Toggle between login and register form\r\n            name: '',\r\n            email: '',\r\n            password: '',\r\n            error: null,\r\n            loginEmail: '',\r\n            loginPassword: '',\r\n        };\r\n    },\r\n    methods: {\r\n        toggleForm() {\r\n            this.isLogin = !this.isLogin;  // Toggle between login and register\r\n        },\r\n        async handleSubmit() {\r\n            this.error = null; // Clear previous errors\r\n            try {\r\n                const response = await axios.post('http://localhost:5555/register', {\r\n                    name: this.name,\r\n                    email: this.email,\r\n                    password: this.password\r\n                });\r\n\r\n                if (response.status === 200) {\r\n                    this.$router.push(\"/\");  // Redirect to login page\r\n                }\r\n            } catch (err) {\r\n                this.error = err.response?.data.message || 'Registration failed. Please try again.';\r\n            }\r\n        },\r\n        async handleLogin() {\r\n            this.error = null;  // Clear previous errors\r\n            try {\r\n                const response = await axios.post('http://localhost:5555/login', {\r\n                    email: this.loginEmail,\r\n                    password: this.loginPassword\r\n                });\r\n\r\n                if (response.status === 200) {\r\n                    this.$router.push(\"/\");  // Redirect to dashboard or user profile\r\n                }\r\n            } catch (err) {\r\n                this.error = err.response?.data.message || 'Login failed. Please try again.';\r\n            }\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Add fade transition animation */\r\n.fade-enter-active, .fade-leave-active {\r\n    transition: opacity 0.5s ease;\r\n}\r\n.fade-enter, .fade-leave-to {\r\n    opacity: 0;\r\n}\r\n\r\n/* Styling for the forms and buttons */\r\n.link-primary {\r\n    color: #573b8a;\r\n    font-weight: bold;\r\n    text-decoration: none;\r\n    transition: color 0.3s ease;\r\n}\r\n\r\n.link-primary:hover {\r\n    color: #6d44b8;\r\n    text-decoration: underline;\r\n}\r\n</style>\r\n"],"mappings":";AA0JA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,mBAAmB;EACzBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE,KAAK;MAAG;MACjBF,IAAI,EAAE,EAAE;MACRG,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,IAAI;MACXC,UAAU,EAAE,EAAE;MACdC,aAAa,EAAE;IACnB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,UAAUA,CAAA,EAAG;MACT,IAAI,CAACP,OAAM,GAAI,CAAC,IAAI,CAACA,OAAO,EAAG;IACnC,CAAC;IACD,MAAMQ,YAAYA,CAAA,EAAG;MACjB,IAAI,CAACL,KAAI,GAAI,IAAI,EAAE;MACnB,IAAI;QACA,MAAMM,QAAO,GAAI,MAAMZ,KAAK,CAACa,IAAI,CAAC,gCAAgC,EAAE;UAChEZ,IAAI,EAAE,IAAI,CAACA,IAAI;UACfG,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACnB,CAAC,CAAC;QAEF,IAAIO,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UACzB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC,EAAG;QAC7B;MACJ,EAAE,OAAOC,GAAG,EAAE;QACV,IAAI,CAACX,KAAI,GAAIW,GAAG,CAACL,QAAQ,EAAEV,IAAI,CAACgB,OAAM,IAAK,wCAAwC;MACvF;IACJ,CAAC;IACD,MAAMC,WAAWA,CAAA,EAAG;MAChB,IAAI,CAACb,KAAI,GAAI,IAAI,EAAG;MACpB,IAAI;QACA,MAAMM,QAAO,GAAI,MAAMZ,KAAK,CAACa,IAAI,CAAC,6BAA6B,EAAE;UAC7DT,KAAK,EAAE,IAAI,CAACG,UAAU;UACtBF,QAAQ,EAAE,IAAI,CAACG;QACnB,CAAC,CAAC;QAEF,IAAII,QAAQ,CAACE,MAAK,KAAM,GAAG,EAAE;UACzB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC,EAAG;QAC7B;MACJ,EAAE,OAAOC,GAAG,EAAE;QACV,IAAI,CAACX,KAAI,GAAIW,GAAG,CAACL,QAAQ,EAAEV,IAAI,CAACgB,OAAM,IAAK,iCAAiC;MAChF;IACJ;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}