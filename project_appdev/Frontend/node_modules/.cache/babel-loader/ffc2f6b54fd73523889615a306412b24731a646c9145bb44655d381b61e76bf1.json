{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"LoginComponent\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      rememberMe: false,\n      error: null\n    };\n  },\n  methods: {\n    async handleLogin() {\n      this.error = null; // Clear previous errors\n      if (!this.email || !this.password) {\n        this.error = \"Please enter both email and password.\";\n        return;\n      }\n      try {\n        const response = await axios.post(\"http://localhost:5555/login\", {\n          email: this.email,\n          password: this.password,\n          rememberMe: this.rememberMe\n        });\n        if (response.data.verified) {\n          this.$router.push(\"/home\");\n        } else {\n          this.error = \"Please verify your Gmail before logging in.\";\n        }\n      } catch (err) {\n        this.error = err.response?.data?.message || \"Login failed. Please try again.\";\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","rememberMe","error","methods","handleLogin","response","post","verified","$router","push","err","message"],"sources":["C:\\laragon\\www\\project_appdev\\Frontend\\src\\views\\loginview.vue"],"sourcesContent":["<template>\r\n  <section class=\"bg-light py-3 py-md-5\">\r\n    <div class=\"container\">\r\n      <div class=\"row justify-content-center\">\r\n        <div class=\"col-12 col-sm-10 col-md-8 col-lg-6 col-xl-5 col-xxl-4\">\r\n          <div class=\"card border border-light-subtle rounded-3 shadow-sm\">\r\n            <div class=\"card-body p-3 p-md-4 p-xl-5\">\r\n              <div class=\"text-center mb-3\">\r\n                <i class=\"bi bi-person-circle\" style=\"font-size: 4rem; color: #007bff;\"></i>\r\n              </div>\r\n              <h2 class=\"fs-6 fw-normal text-center text-secondary mb-4\">Sign in to your account</h2>\r\n\r\n              <!-- Display error message if exists -->\r\n              <div v-if=\"error\" class=\"alert alert-danger\" role=\"alert\">\r\n                {{ error }}\r\n              </div>\r\n\r\n              <!-- Login form -->\r\n              <form @submit.prevent=\"handleLogin\">\r\n                <div class=\"row gy-2 overflow-hidden\">\r\n                  <!-- Email Field -->\r\n                  <div class=\"col-12\">\r\n                    <div class=\"form-floating mb-3\">\r\n                      <input\r\n                        type=\"email\"\r\n                        class=\"form-control\"\r\n                        v-model=\"email\"\r\n                        id=\"email\"\r\n                        placeholder=\"name@example.com\"\r\n                        required\r\n                      />\r\n                      <label for=\"email\">Email</label>\r\n                    </div>\r\n                  </div>\r\n\r\n                  <!-- Password Field -->\r\n                  <div class=\"col-12\">\r\n                    <div class=\"form-floating mb-3\">\r\n                      <input\r\n                        type=\"password\"\r\n                        class=\"form-control\"\r\n                        v-model=\"password\"\r\n                        id=\"password\"\r\n                        placeholder=\"Password\"\r\n                        required\r\n                      />\r\n                      <label for=\"password\">Password</label>\r\n                    </div>\r\n                  </div>\r\n\r\n                  <!-- Keep me logged in -->\r\n                  <div class=\"col-12\">\r\n                    <div class=\"form-check text-start mb-3\">\r\n                      <input\r\n                        class=\"form-check-input\"\r\n                        type=\"checkbox\"\r\n                        v-model=\"rememberMe\"\r\n                        id=\"rememberMe\"\r\n                      />\r\n                      <label class=\"form-check-label text-secondary\" for=\"rememberMe\">\r\n                        Keep me logged in\r\n                      </label>\r\n                    </div>\r\n                  </div>\r\n\r\n                  <!-- Submit Button -->\r\n                  <div class=\"col-12\">\r\n                    <div class=\"d-grid my-3\">\r\n                      <button class=\"btn btn-primary btn-lg\" type=\"submit\">Log in</button>\r\n                    </div>\r\n                  </div>\r\n\r\n                  <!-- Forgot password link -->\r\n                  <div class=\"col-12\">\r\n                    <p class=\"m-0 text-secondary text-center\">\r\n                      Forgot your password?\r\n                      <a href=\"#!\" class=\"link-primary text-decoration-none\">Reset here</a>\r\n                    </p>\r\n                  </div>\r\n\r\n                  <!-- Don't have an account -->\r\n                  <div class=\"col-12\">\r\n                    <p class=\"m-0 text-secondary text-center\">\r\n                      Don't have an account?\r\n                      <router-link to=\"/register\" class=\"link-primary text-decoration-none\">Sign up</router-link>\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"LoginComponent\",\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\",\r\n      rememberMe: false,\r\n      error: null,\r\n    };\r\n  },\r\n  methods: {\r\n    async handleLogin() {\r\n      this.error = null; // Clear previous errors\r\n      if (!this.email || !this.password) {\r\n        this.error = \"Please enter both email and password.\";\r\n        return;\r\n      }\r\n      try {\r\n        const response = await axios.post(\"http://localhost:5555/login\", {\r\n          email: this.email,\r\n          password: this.password,\r\n          rememberMe: this.rememberMe,\r\n        });\r\n        if (response.data.verified) {\r\n          this.$router.push(\"/home\");\r\n        } else {\r\n          this.error = \"Please verify your Gmail before logging in.\";\r\n        }\r\n      } catch (err) {\r\n        this.error =\r\n          err.response?.data?.message || \"Login failed. Please try again.\";\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nbody {\r\n  margin: 0;\r\n  padding: 0;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  min-height: 100vh;\r\n  font-family: \"Jost\", sans-serif;\r\n}\r\n\r\n.bg-light {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  min-height: 100vh;\r\n  background: transparent;\r\n}\r\n\r\n.card {\r\n  border: none;\r\n  border-radius: 10px;\r\n  box-shadow: 5px 20px 50px rgba(0, 0, 0, 0.6);\r\n  overflow: hidden;\r\n  background: linear-gradient(to bottom right, rgba(255, 255, 255, 0.8), rgba(255, 255, 255, 0.5));\r\n  backdrop-filter: blur(10px);\r\n}\r\n\r\n.card-body {\r\n  padding: 30px 20px;\r\n  color: #333;\r\n  text-align: center;\r\n}\r\n\r\n.fs-6 {\r\n  font-size: 1.2rem;\r\n  font-weight: 600;\r\n  color: #24243e;\r\n}\r\n\r\n.form-control {\r\n  border: 1px solid rgba(255, 255, 255, 0.3);\r\n  background-color: rgba(255, 255, 255, 0.8);\r\n  border-radius: 8px;\r\n  font-size: 1rem;\r\n  padding: 0.75rem;\r\n  color: #24243e;\r\n  transition: border-color 0.3s, background-color 0.3s;\r\n}\r\n\r\n.form-control:focus {\r\n  border-color: #007bff;\r\n  background-color: rgba(255, 255, 255, 1);\r\n  box-shadow: 0 0 5px rgba(0, 123, 255, 0.5);\r\n}\r\n\r\n.btn-primary {\r\n  background: linear-gradient(to right, #6d44b8, #573b8a);\r\n  border: none;\r\n  color: #fff;\r\n  font-size: 1.1rem;\r\n  font-weight: bold;\r\n  padding: 0.75rem;\r\n  border-radius: 8px;\r\n  transition: background 0.3s ease-in-out, transform 0.2s ease;\r\n}\r\n\r\n.btn-primary:hover {\r\n  background: linear-gradient(to right, #573b8a, #6d44b8);\r\n  transform: translateY(-2px);\r\n}\r\n\r\n.link-primary {\r\n  color: #573b8a;\r\n  font-weight: bold;\r\n  text-decoration: none;\r\n  transition: color 0.3s ease;\r\n}\r\n\r\n.link-primary:hover {\r\n  color: #6d44b8;\r\n  text-decoration: underline;\r\n}\r\n\r\n.alert {\r\n  font-size: 0.9rem;\r\n  font-weight: bold;\r\n  background: rgba(255, 0, 0, 0.1);\r\n  color: #ff0000;\r\n  border: 1px solid rgba(255, 0, 0, 0.2);\r\n  border-radius: 8px;\r\n  padding: 10px;\r\n}\r\n</style>\r\n"],"mappings":";AAkGA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,KAAK;MACjBC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI,CAACF,KAAI,GAAI,IAAI,EAAE;MACnB,IAAI,CAAC,IAAI,CAACH,KAAI,IAAK,CAAC,IAAI,CAACC,QAAQ,EAAE;QACjC,IAAI,CAACE,KAAI,GAAI,uCAAuC;QACpD;MACF;MACA,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMT,KAAK,CAACU,IAAI,CAAC,6BAA6B,EAAE;UAC/DP,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,UAAU,EAAE,IAAI,CAACA;QACnB,CAAC,CAAC;QACF,IAAII,QAAQ,CAACP,IAAI,CAACS,QAAQ,EAAE;UAC1B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,OAAO,CAAC;QAC5B,OAAO;UACL,IAAI,CAACP,KAAI,GAAI,6CAA6C;QAC5D;MACF,EAAE,OAAOQ,GAAG,EAAE;QACZ,IAAI,CAACR,KAAI,GACPQ,GAAG,CAACL,QAAQ,EAAEP,IAAI,EAAEa,OAAM,IAAK,iCAAiC;MACpE;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}